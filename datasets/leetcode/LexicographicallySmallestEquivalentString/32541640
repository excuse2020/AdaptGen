class Solution {
	int find(int[] parent, int x) {
		if (parent[x] != x) {
			parent[x] = find(parent,parent[x]);
		}
		return parent[x];
	}
	String smallestEquivalentString(String s1, String s2, String baseStr) {
		int parent = new int[26];
		for (int i = 0; i < 26; i++) {
			parent[i] = i;
		}
		int i = 0;
		int c;
	}
}