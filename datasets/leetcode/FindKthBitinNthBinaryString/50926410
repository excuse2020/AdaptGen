class Solution {
	String Test(int n) {
		if (n == 1) {
			return "0";
		}
		String answer = Test(n - 1) + "1";
		return answer;
	}
	char findKthBit(int n, int k) {
		String answer = Test(n);
		return answer.charAt(k - 1);
	}
	String rev_invert(String test) {
		StringBuilder str = new StringBuilder();
		int i = 0;
	}
}