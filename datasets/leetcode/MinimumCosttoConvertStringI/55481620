class Solution {
	long minimumCost(String source, String target, char[] original, char[] changed, int[] cost) {
		long result = 0;
		int INF = (int) Integer.MAX_VALUE / 10;
		int[][] adjacencyMatrix = new int[26][26];
		for (int i = 0; i < 26; i++) {
			for (int j = 0; j < 26; j++) {
				adjacencyMatrix[i][j] = (i == j) ? 0 : INF;
			}
		}
		int i = 0;
	}
}