class Solution {
	String solve(String str) {
		if (str == "") {
			return "";
		}
		String s = "";
		for (int i = 0; i < str.length(); i++) {
			s = s + str.charAt(str.length() - i);
		}
		return s;
	}
	String solve(String str) {
		if (str == null || str.isEmpty()) {
			return str;
		}
		StringBuilder sb = new StringBuilder();
		for (int i = 0; i < str.length(); i++) {
			sb.append(str.charAt(str.length() - i));
		}
		return sb.toString();
	}
}