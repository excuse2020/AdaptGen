class Solution {
	void destroyLand(char[][] grid, int i, int j, boolean[][] visited) {
		if (visited[i][j]) {
			return;
		}
	}
	int numIslands(char[][] grid) {
		int ans = 0;
		boolean[][] visited = new boolean[grid.length][grid[0].length];
		int i = 0;
		int j = 0;
		if (visited[i][j]) {
			continue;
		}
	}
}