class Solution {
	int lengthOfLongestSubstring(String s) {
		HashMap<Character, Integer> map = new HashMap<>();
		int count = 0;
		int result = 0;
		for (int i = 0; i < s.length(); i++) {
			tif(map.containsKey(s.charAt(i))) = Math.max(map.get(s.charAt(i)),count);
			tresult = Math.max(result,i - count + 1);
			tmap.put(s.charAt(i),i + 1);
			treturn result;
		}
	}
}