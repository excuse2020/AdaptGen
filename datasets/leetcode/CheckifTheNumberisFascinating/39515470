class Solution {
	boolean isFascinating(int n) {
		int n1 = 2 * n;
		int n2 = 3 * n;
		String f = Integer.toString(n1).concat(Integer.toString(n2));
		String str1 = Integer.toString(n);
		String ans = f.concat(str1);
		HashMap<Character, Integer> hm = new HashMap<>();
		int i = 0;
		char ch = ans.charAt(i);
	}
}