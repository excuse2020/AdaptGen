class Solution {
	List<String> validStrings(int n) {
		List<String> res = new ArrayList<>();
		int i = 0;
		StringBuilder sb = new StringBuilder();
		boolean isValid = true;
		for (int j = 0; j < n; j++) {
			int bit = 1 & (i >> j);
			sb.append(bit);
		}
		char prev = sb.charAt(0);
		int j = 1;
	}
}