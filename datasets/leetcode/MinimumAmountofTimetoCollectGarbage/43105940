class Solution {
	int garbageCollection(String[] g, int[] travel) {
		boolean cnt_m = false, cnt_p = false, cnt_g = false;
		int idx_m = 0, idx_p = 0, idx_g = 0;
		int res = 0, edge_cnt = 0;
		int i = 0;
		String cur = g[i];
		res += cur.length();
		edge_cnt += i > 0 ? travel[i - 1] : 0;
		cnt_m = false;
		cnt_p = false;
		cnt_g = false;
		int j = 0;
		char ch = cur.charAt(j);
	}
}