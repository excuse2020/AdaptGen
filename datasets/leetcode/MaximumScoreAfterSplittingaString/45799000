class Solution {
	int calculateScore(String s, char target, int length) {
		if (s.length() == length || s.isEmpty()) {
			return 0;
		}
		int score = 0;
		for (char curr: s.toCharArray()) {	
			if (curr == target) {
				score++;
			}
		}
		return score;
	}
	int maxScore(String s) {
		int maxScore = Integer.MIN_VALUE;
		int i = 0;
	}
}