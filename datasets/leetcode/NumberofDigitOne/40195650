class Solution {
	int countDigitOne(int n) {
		String s = Integer.toString(n);
		int len = s.length();
		int res = 0;
		int multi = (int) Math.pow(10,len - 1);
		int i = 0;
		char c = s.charAt(i);
		String l = s.substring(0,i);
		String r = s.substring(i + 1,len);
		int left = l.equals("") ? 0 : Integer.valueOf(l);
		int right = r.equals("") ? 0 : Integer.valueOf(r);
	}
}