class Solution {
	int beautySum(String s) {
		int sum = 0;
		char[] charArr = s.toCharArray();
		int i = 0;
		int[] arr = new int[26];
		int j = i;
	}
	int getDiff(int[] arr) {
		int max = 0, min = 501;
		for (int n: arr) {	
			if (n != 0) {
				min = Math.min(min,n);
				max = Math.max(max,n);
			}
		}
		return max - min;
	}
}