class Solution {
	List<String> letterCombinations(String up) {
		return rec("",up);
	}
	List<String> rec(String pro, String up) {
		if (up.isEmpty()) {
			List<String> list = new ArrayList<>();
			if (!pro.isEmpty()) {
				list.add(pro);
				return list;
			} 
			else {
				return list;
			}
		}
		int dig;
	}
}