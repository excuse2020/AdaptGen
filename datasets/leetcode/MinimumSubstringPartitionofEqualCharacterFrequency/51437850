class Solution {
	int minimumSubstringsInPartition(String s) {
		int arr = new int[s.length()];
		Arrays.fill(arr,-1);
		return task(s.length() - 1,s,arr);
	}
	int task(int ind, String s, int arr) {
		if (ind < 0) {
			return 0;
		}
		if (arr[ind] != -1) {
			return arr[ind];
		}
		int a = new int[26];
		int ans = 10000;
		int j = ind;
	}
}