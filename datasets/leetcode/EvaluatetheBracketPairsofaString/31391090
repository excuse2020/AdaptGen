class Solution {
	String evaluate(String s, List<List<String>> knowledge) {
		StringBuilder sb = new StringBuilder();
		HashMap<String, String> map = new HashMap<>();
		for (List<String> l: knowledge) {	
			String key = l.get(0);
			String value = l.get(1);
			map.put(key,value);
		}
		int i = 0;
		char ch = s.charAt(i);
	}
}