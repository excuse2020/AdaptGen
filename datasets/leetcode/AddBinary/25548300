class Solution {
	String addBinary(String a, String b) {
		StringBuilder s = new StringBuilder("");
		int x = a.length();
		int y = b.length();
		int diff = x - y;
		if (diff < 0) {
			diff = diff * -1;
		}
		if (x > y) {
			while (diff > 0) {
				diff--;
				b = "0" + b;
			}
		} 
		else {
			while (diff > 0) {
				diff--;
				a = "0" + a;
			}
		}
		String carry = "";
		int i = a.length() - 1;
	}
}