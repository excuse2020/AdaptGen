class Solution {
	String longestCommonPrefix(String[] strs) {
		if (strs == null || strs.length == 0) {
			return "";
		}
		int minLen = Integer.MAX_VALUE;
		for (String str: strs) {	
			if (minLen > str.length()) {
				minLen = str.length();
			}
		}
		if (minLen == 0) {
			return "";
		}
		int j = 0;
		char prev;
	}
}