class Solution {
	int canBeTypedWords(String text, String brokenLetters) {
		int res = 0;
		if (text == null || text.isEmpty()) {
			return res;
		}
		int i = 0;
		int size = text.length() - 1;
		boolean isBroken;
		boolean isNextSpace;
		char cur = text.charAt(i);
		isNextSpace = false;
		isBroken = false;
		cur = text.charAt(i);
		if (brokenLetters.indexOf(cur) != -1) {
			isBroken = true;
		}
	}
}