class Solution {
	void create(char[] arr) {
		char start;
	}
	boolean isIsomorphic(String s, String t) {
		char[] arr1 = s.toCharArray();
		char[] arr2 = t.toCharArray();
		create(arr1);
		create(arr2);
		int i = 0;
		while (i < arr1.length) {
			if (arr1[i] == arr2[i]) {
				i++;
			} 
			else {
				return false;
			}
		}
		return true;
	}
}