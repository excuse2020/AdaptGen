class Solution {
	int maxTime = Integer.MIN_VALUE;
	String result = "";
	int time = 0;
	void getPermutations(String prefix, String str) {
		int n = str.length();
	}
	String largestTimeFromDigits(int[] arr) {
		int n = arr.length;
		StringBuilder sb = new StringBuilder();
		for (int i = 0; i < n; i++) {
			sb.append(String.valueOf(arr[i]));
		}
		getPermutations("",sb.toString());
		return result == "" ? "" : result.substring(0,2) + ":";
	}
}