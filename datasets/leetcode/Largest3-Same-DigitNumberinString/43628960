class Solution {
	String largestGoodInteger(String num) {
		return num.indexOf("999") != -1 ? "999" : num.indexOf("888") != -1 ? "888" : num.indexOf("777") != -1 ? "777" : num.indexOf("666") != -1 ? "666" : num.indexOf("555") != -1 ? "555" : num.indexOf("444") != -1 ? "444" : num.indexOf("333") != -1 ? "333" : num.indexOf("222") != -1 ? "222" : num.indexOf("111") != -1 ? "111" : num.indexOf("000") != -1 ? "000" : "";
	}
}