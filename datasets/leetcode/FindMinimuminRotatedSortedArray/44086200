class Solution {
	int findMin(int[] nums) {
		int start = 0;
		int end = nums.length - 1;
		int ans = Integer.MAX_VALUE;
		while (start <= end) {
			int mid = start + (end - start) / 2;
			if (nums[mid] < nums[end]) {
				ans = ans > nums[mid] ? nums[mid] : ans;
				end = mid;
			} 
			else {
				ans = ans > nums[start] ? nums[start] : ans;
				start = mid + 1;
			}
		}
		return ans;
	}
}