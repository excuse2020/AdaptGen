class Solution {
	int convertString(String s) {
		int num = 0;
		int i = 0;
	}
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		if (convertString(firstWord) + convertString(secondWord) == convertString(targetWord)) {
			return true;
		} 
		else {
			return false;
		}
	}
}