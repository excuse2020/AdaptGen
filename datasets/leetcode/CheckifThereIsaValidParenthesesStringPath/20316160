class Solution {
	Boolean dp;
	boolean hasValidPath(char[][] grid) {
		dp = new Boolean[grid.length][grid[0].length][grid.length + grid[0].length];
		return helper(grid,0,0,0);
	}
	boolean helper(char[][] grid, int i, int j, int start) {
		if (i >= grid.length || j >= grid[0].length) {
			return false;
		}
	}
}