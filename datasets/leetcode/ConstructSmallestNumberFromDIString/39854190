class Solution {
	String ans = "";
	void reverse(int nums, int l, int h) {
		while (l < h) {
			int tmp = nums[l];
			nums[l] = nums[h];
			nums[h] = tmp;
			h--;
			l++;
		}
	}
	String smallestNumber(String pattern) {
		int n = pattern.length();
		int num = new int[n + 1];
		for (int i = 0; i < num.length; i++) {
			num[i] = i + 1;
		}
		int i = 0;
	}
}