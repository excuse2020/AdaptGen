class Solution {
	int maxVowels(String s, int k) {
		Map<Character, Integer> m = new HashMap<>();
		int i = 0, j = 0, n = s.length(), cnt = 0, windowLength = 0;
		m.put(s.charAt(j),m.getOrDefault(s.charAt(j),0) + 1);
		if ((j - i) >= k) {
			m.put(s.charAt(i),m.getOrDefault(s.charAt(i),0) - 1);
			i++;
		}
		j++;
		int cntTemp;
	}
}