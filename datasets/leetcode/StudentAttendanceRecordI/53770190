class Solution {
	boolean absentCheck(String s) {
		int count = 0;
		int i = 0;
		if (count >= 2) {
			return false;
		}
	}
	boolean checkRecord(String s) {
		if (absentCheck(s) && lateCheck(s)) {
			return true;
		}
		return false;
	}
	boolean lateCheck(String s) {
		int count = 0;
		int conseq = 0;
		int i = 0;
		if (conseq >= 3) {
			return false;
		}
	}
}