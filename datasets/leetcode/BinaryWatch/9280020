class Solution {
	int count(String s, char l) {
		int count = 0;
		for (int i = 0; i < s.length(); i++) {
			char c = s.charAt(i);
			if (c == l) {
				count++;
			}
		}
		return count;
	}
	String minutes(int x) {
		String ret = "";
		if (x <= 9) {
			ret = ret + "0";
		} 
		else {
			ret = ret + x;
		}
		return ret;
	}
	List<String> readBinaryWatch(int num) {
		ArrayList<String> list = new ArrayList<String>();
		int hour = 0;
		int min = 0;
		String hours = Integer.toBinaryString(hour);
		String mins = Integer.toBinaryString(min);
	}
}