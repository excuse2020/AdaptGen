class Solution {
	List<String> printVertically(String s) {
		String[] st = s.split(" ");
		int n = st.length;
		List<String> res = new ArrayList<>();
		int max = 0;
		for (int i = 0; i < n; i++) {
			if (st[i].length() > max) {
				max = st[i].length();
			}
		}
		int i = 0;
		String r = "";
		int j = 0;
		String m = st[j];
		char c;
	}
	String removetr(String s) {
		int i = s.length() - 1;
	}
}