class Solution {
	int countVowelSubstrings(String word) {
		int n = word.length(), count = 0;
		int left = 0, right = word.length();
		StringBuilder sb = new StringBuilder();
		int i = left;
		char c = word.charAt(i);
	}
	boolean isValid(String s) {
		if (s.contains("a") && s.contains("e")) {
			return true;
		}
		return false;
	}
}