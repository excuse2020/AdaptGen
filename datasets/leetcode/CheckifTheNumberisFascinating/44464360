class Solution {
	boolean isFascinating(int n) {
		int[] temp = new int[9];
		String num1 = String.valueOf(n);
		String num2 = String.valueOf(2 * n);
		String num3 = String.valueOf(3 * n);
		StringBuilder sb = new StringBuilder();
		sb.append(num1);
		sb.append(num2);
		sb.append(num3);
		int i = 0;
	}
}