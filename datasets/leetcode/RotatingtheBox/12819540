class Solution {
	char[][] rotate(char[][] input) {
		int n = input.length;
		int m = input[0].length;
		char[][] output = new char[m][n];
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < m; j++) {
				output[j][n - 1 - i] = input[i][j];
			}
		}
		return output;
	}
	char[][] rotateTheBox(char[][] box) {
		int surface;
		int lastIndex;
		boolean test = true;
		int i = 0;
		lastIndex = 0;
		surface = box[0].length - 1;
		int j = 1;
		lastIndex = box[i].length - j;
	}
}