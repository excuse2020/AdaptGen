class Solution {
	Integer[] dp;
	int f(int idx, String s) {
		if (idx >= s.length()) {
			return 0;
		}
		int[] freq = new int[26];
		int ans = 10001;
		if (dp[idx] != null) {
			return dp[idx];
		}
		int i = idx;
	}
	int minimumSubstringsInPartition(String s) {
		dp = new Integer[s.length()];
		return f(0,s);
	}
}