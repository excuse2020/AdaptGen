class Solution {
	int makeTheIntegerZero(int num1, int num2) {
		if (num1 == 0) {
			return 0;
		}
		if (num1 == num2) {
			return -1;
		}
		int cnt = 1;
		long min = 0;
		long num = num1;
		num -= num2;
		int bitCnt = 0;
		int lowestbit = -1;
		String b = Long.toBinaryString(num);
		int i = b.length() - 1;
	}
}