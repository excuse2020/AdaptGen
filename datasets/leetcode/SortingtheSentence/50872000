class Solution {
	void helper(String sn, String[] ans) {
		int n = sn.length() - 1;
		int k;
	}
	String sortSentence(String s) {
		String[] s1 = s.split(" ");
		String[] ans = new String[s1.length];
		for (String sn: s1) {	
			helper(sn,ans);
		}
		String answer = String.join(" ",ans);
		return answer;
	}
}