class Solution {
	boolean helper(char[][] board, int row, int col) {
		if (row == board.length) {
			return true;
		}
		int nrow = 0;
		int ncol = 0;
		if (col != board.length - 1) {
			nrow = row;
			ncol = col + 1;
		} 
		else {
			nrow = row + 1;
			ncol = 0;
		}
	}
	boolean issafe(char[][] board, int row, int col, int number) {
		int i = 0;
	}
	void solveSudoku(char[][] board) {
		helper(board,0,0);
	}
}