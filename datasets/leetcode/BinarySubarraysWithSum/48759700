class Solution {
	int numSubarraysWithSum(int[] nums, int goal) {
		return numSubarraysWithSumAtmostK(nums,goal) - numSubarraysWithSumAtmostK(nums,goal - 1);
	}
	int numSubarraysWithSumAtmostK(int[] nums, int goal) {
		@r int start = 0;
		@r int end = 0;
		@r int count = 0;
		@r int sum = 0;
		sum += nums[end];
	}
}