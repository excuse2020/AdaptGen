class Solution {
	int ansFunction(int n, int[] dp) {
		if (n == 0 || n == 1) {
			return 1;
		}
		if (dp[n] != -1) {
			return dp[n];
		} 
		else {
			return dp[n] = ansFunction(n - 1,dp) + ansFunction(n - 2,dp);
		}
	}
	int climbStairs(int n) {
		int[] dp = new int[n + 1];
		Arrays.fill(dp,-1);
		return ansFunction(n,dp);
	}
}