class Solution {
	void reverse(int[] arr, int left, int right) {
		while (left < right) {
			int temp = arr[left];
			arr[left] = arr[right];
			arr[right] = temp;
			left++;
			right--;
		}
	}
	String smallestNumber(String pattern) {
		int n = pattern.length() + 1;
		String ans = "";
		int[] arr = new int[n];
		for (int i = 0; i < n; i++) {
			arr[i] = i + 1;
		}
		int i = 0;
		int temp = i;
	}
}