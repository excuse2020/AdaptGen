class Solution {
	int getFalseCount(String answerKey, int k) {
		int zeroCnt = 0;
		int n = answerKey.length();
		int ans = 0;
		int start = 0;
		int end = 0;
	}
	int getTrueCount(String answerKey, int k) {
		int zeroCnt = 0;
		int n = answerKey.length();
		int ans = 0;
		int start = 0;
		int end = 0;
	}
	int maxConsecutiveAnswers(String answerKey, int k) {
		return Math.max(getTrueCount(answerKey,k),getFalseCount(answerKey,k));
	}
}