class Solution {
	int[] countPoints(int[][] points, int[][] queries) {
		int[] arr = new int[queries.length];
		for (int i = 0; i < queries.length; i++) {
			int h = queries[i][0];
			int k = queries[i][1];
			int r = queries[i][2];
			for (int j = 0; j < points.length; j++) {
				int x = points[j][0];
				int y = points[j][1];
				if ((x - h) * (x - h) + (y - k) * (y - k) <= r * r) {
					arr[i]++;
				}
			}
		}
		return arr;
	}
}