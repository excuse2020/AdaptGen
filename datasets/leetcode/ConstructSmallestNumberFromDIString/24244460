class Solution {
	boolean helper(int len, String pattern, StringBuffer[] ans, boolean used) {
		int i = 0;
	}
	String smallestNumber(String pattern) {
		StringBuffer[] ans = new StringBuffer[1];
		ans[0] = new StringBuffer();
		helper(pattern.length() + 1,pattern,ans,new boolean[10]);
		return ans[0].toString();
	}
}