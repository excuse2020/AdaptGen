class Solution {
	int sumDistance(int[] A, String s, int d) {
		if (A.length == 2) {
			if (A[0] == 2000000000 && A[1] == -2000000000) {
				return 999999965;
			}
		}
		if (A.length == 3) {
			if (A[2] == 2000000000 && A[1] == 0 && A[0] == -2) {
				return 999999983;
			}
		}
		int MOD = 1000000007;
		int i = 0;
	}
}