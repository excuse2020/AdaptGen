class Solution {
	int MOD = 1000000007;
	int maxLength;
	int[] memo;
	boolean isValidString(String sub, int k) {
		return Long.parseLong(sub) <= k;
	}
	int numberOfArrays(String string, int index, int k) {
		if (index == string.length()) {
			return 1;
		}
	}
	int numberOfArrays(String s, int k) {
		memo = new int[s.length()];
		maxLength = Math.min((k + "").length(),(Integer.MAX_VALUE + "").length());
		return numberOfArrays(s,0,k);
	}
}