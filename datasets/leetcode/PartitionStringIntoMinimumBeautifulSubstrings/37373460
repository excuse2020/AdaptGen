class Solution {
	int dp;
	int max = (int) 1e9;
	Set<String> st;
	int helper(String s, int idx) {
		if (idx >= s.length()) {
			return 0;
		}
	}
	int minimumBeautifulSubstrings(String s) {
		dp = new int[s.length()];
		Arrays.fill(dp,-1);
		int ans = helper(s,0);
		return ans >= max ? -1 : ans;
	}
}