class Solution {
	boolean strongPasswordCheckerII(String password) {
		if (password.length() < 8) {
			return false;
		}
		boolean lowercase = false, uppercase = false, digit = false, special = false;
		int i = 0;
		char ch = password.charAt(i);
		digit = true;
		if (Character.isUpperCase(ch)) {
			uppercase = true;
		} 
		else {
			lowercase = true;
		}
	}
}