class Solution {
	int countMatches(List<List<String>> items, String ruleKey, String ruleValue) {
		int rk_index = -1;
		switch (ruleKey) {
			case "type":
			case "color":
			case "name":
		}
		int n_items = 0;
		for (int i = 0; i < items.size(); i++) {
			if (items.get(i).get(rk_index).equals(ruleValue)) {
				n_items++;
			}
		}
		return n_items;
	}
}