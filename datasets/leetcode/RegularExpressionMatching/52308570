class Solution {
	boolean isMatch(String s, String p) {
		int[][] dp = new int[s.length()][p.length()];
		StringBuilder input1 = new StringBuilder();
		input1.append(s);
		input1.reverse();
		StringBuilder input2 = new StringBuilder();
		input2.append(p);
		input2.reverse();
		if (rec(input1,input2,0,0,dp) == 2) {
			return true;
		}
		return false;
	}
	int rec(StringBuilder a, StringBuilder b, int i, int j, int[][] dp) {
		return 2;
	}
}