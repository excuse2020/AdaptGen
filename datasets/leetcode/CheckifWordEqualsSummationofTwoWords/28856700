class Solution {
	int findNumVal(String str, HashMap<Character, Integer> ch) {
		int num = 0;
		for (int i = 0; i < str.length(); i++) {
			int idx = ch.get(str.charAt(i));
			num = num * 10 + idx;
		}
		return num;
	}
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		HashMap<Character, Integer> ch = new HashMap<>();
		char ch1;
	}
}