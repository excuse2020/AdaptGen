class Solution {
	Integer dp;
	int minFlipsMonoIncr(String s) {
		dp = new Integer[s.length()][2];
		char arr = s.toCharArray();
	}
	int minimize(char[] arr, int idx, char prev) {
		if (idx >= arr.length) {
			return 0;
		}
		if (dp[idx][prev - 48] != null) {
			return dp[idx][prev - 48];
		}
		int flip = Integer.MAX_VALUE;
		int doNotFlip = Integer.MAX_VALUE;
	}
}