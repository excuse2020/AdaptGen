class Solution {
	int lengthOfLongestSubstring(String s) {
		int left = 0;
		int right = 0;
		int max = -1;
		int n = s.length();
		if (n == 0) {
			return 0;
		}
		HashSet<Character> set = new HashSet<>();
		tif(!set.contains(s.charAt(right))).add(s.charAt(right++));
		tmax = Math.max(max,right - left);
		tset.remove(s.charAt(left++));
		tmax = Math.max(max,right - left);
		treturn max;
	}
}