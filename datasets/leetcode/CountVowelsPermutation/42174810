class Solution {
	int countVowelPermutation(int n) {
		HashMap<String, Integer> dp = new HashMap<>();
		if (n == 0) {
			return 0;
		}
	}
	int helper(char ch, int n, HashMap<String, Integer> dp) {
		String str = "" + ch;
		if (dp.containsKey(str)) {
			return dp.get(str);
		}
		int mod = (int) Math.pow(10,9) + 7;
		if (n == 0) {
			return 1;
		}
		int count = 0;
	}
}