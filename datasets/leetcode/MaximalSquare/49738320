class Solution {
	int maximalSquare(char[][] matrix) {
		int m = matrix.length;
		int n = matrix[0].length;
		int maxside = 0;
		int dp = new int[m + 1][n + 1];
		for (int i = 0; i < m + 1; i++) {
			dp[i][n] = 0;
		}
		for (int i = 0; i < n + 1; i++) {
			dp[m][i] = 0;
		}
		int i = m - 1;
		int j = n - 1;
		char s = matrix[i][j];
	}
}