class Solution {
	int longestIdealString(String s, int k) {
		int[][] dp = new int[s.length()][130];
		for (int[] d: dp) {	
			Arrays.fill(d,-1);
		}
	}
	int maxSubseq(String s, int k, int[][] dp, int i, char ch) {
		if (i == s.length()) {
			return 0;
		}
		if (dp[i][ch] != -1) {
			return dp[i][ch];
		}
	}
}