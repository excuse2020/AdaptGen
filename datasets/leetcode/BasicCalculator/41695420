class Solution {
	int currentIndex = 0;
	int calculate(String s) {
		Stack<Integer> signStack = new Stack<>();
		int result = 0;
		signStack.push(1);
		s = cleanup(s);
		char c = s.charAt(currentIndex);
		int digit = fetchNumberFromCurrentIndex(s);
		result += signStack.peek() * digit;
	}
	String cleanup(String input) {
		StringBuilder sb = new StringBuilder();
	}
	int fetchNumberFromCurrentIndex(String input) {
		int digit = 0;
	}
}