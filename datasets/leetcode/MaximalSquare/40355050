class Solution {
	int findMin(int a, int b, int c) {
		a = Math.min(a,b);
		return Math.min(a,c);
	}
	int maximalSquare(char[][] matrix) {
		int row = matrix.length;
		int col = matrix[0].length;
		int[][] mat = new int[row + 1][col + 1];
		int max = 0;
		for (int i = 0; i < row; i++) {
			mat[i][0] = 0;
		}
		for (int i = 0; i < col; i++) {
			mat[0][i] = 0;
		}
		int i = 1;
		int j = 1;
	}
}