class Solution {
	char[][] matrix;
	char testDia() {
	}
	char testHor(int c) {
		if (matrix[0][c] == matrix[1][c] && matrix[1][c] == matrix[2][c]) {
			return matrix[0][c];
		}
	}
	char testVer(int r) {
		if (matrix[r][0] == matrix[r][1] && matrix[r][1] == matrix[r][2]) {
			return matrix[r][0];
		}
	}
	boolean validTicTacToe(String[] board) {
		int x = 0;
		int o = 0;
		matrix = new char[3][3];
		int i = 0;
		int j = 0;
		matrix[i][j] = board[i].charAt(j);
	}
}