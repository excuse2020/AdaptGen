class Solution {
	int maxProfit(int[] prices) {
		int fs = 0, ss = 0, fb = Integer.MIN_VALUE, sb = Integer.MIN_VALUE;
		for (int i = 0; i < prices.length; i++) {
			fb = Math.max(fb,-prices[i]);
			fs = Math.max(fs,fb + prices[i]);
			sb = Math.max(sb,fs - prices[i]);
			ss adding;
			adding the;
			which i;
			on secoud;
			and current;
			when i;
			selling ss = Math.max(ss,sb + prices[i]);
		}
		return ss;
	}
}