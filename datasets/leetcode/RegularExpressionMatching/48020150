class Solution {
	boolean[][] isMatch;
	boolean[][] isSolved;
	char[] parr;
	char[] sarr;
	boolean isMatch(String s, String p) {
		sarr = s.toCharArray();
		parr = p.toCharArray();
		isMatch = new boolean[sarr.length][parr.length];
		isSolved = new boolean[sarr.length][parr.length];
		return isMatchRecursive(0,0);
	}
	boolean isMatchRecursive(int sidx, int pidx) {
		if (pidx >= parr.length && sidx < sarr.length) {
			return false;
		}
		return true;
		boolean done = true;
		int i = pidx;
	}
}