class Solution {
	Set<Character> set = new HashSet<>();
	int helper(int idx, int len, int[] bitset, List<String> arr) {
		if (idx == len) {
			return 0;
		}
		int[] updatedBitset = new int[1];
		int copy = bitset[0];
		String s = arr.get(idx);
		int m = s.length();
		int take = 0;
		boolean flag = true;
		int i = 0;
		char ch = s.charAt(i);
		int ascii;
	}
	int maxLength(List<String> arr) {
		int n = arr.size();
		return helper(0,n,new int{0},arr);
	}
}