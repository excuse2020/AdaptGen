class Solution {
	String replaceSpace(String s) {
		s = s + "1";
		String[] split = s.split(" ");
		String s2 = "";
		int i = 0;
		for (i = 0; i < split.length - 1; i++) {
			s2 = s2 + split[i];
		}
		if (split[i].equals("1")) {
		} 
		else {
			s2 = s2 + split[i];
			s2 = s2.substring(0,s2.length() - 1);
		}
		return s2;
	}
}