class Solution {
	Integer[][][] dp;
	int minimizeConcatenatedLength(String[] words) {
		int n = words.length;
		if (n == 1) {
			return words[0].length();
		}
		dp = new Integer[n + 1][26][26];
		int ps;
	}
	int solve(int i, int n, int ps, int pe, String[] words) {
		if (i == n) {
			return 0;
		}
		if (dp[i][ps][pe] != null) {
			return dp[i][ps][pe];
		}
		String cur = words[i];
		int cs;
	}
}