class Solution {
	int heightChecker(int[] heights) {
		int size = heights.length;
		int[] idealPhoto = heights.clone();
		Arrays.sort(idealPhoto);
		int counter = 0;
		for (int i = 0; i < size; i++) {
			if (idealPhoto[i] != heights[i]) {
				counter++;
			} 
			else {
			}
		}
		return counter;
	}
}