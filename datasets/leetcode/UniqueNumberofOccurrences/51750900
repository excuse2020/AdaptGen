class Solution {
	boolean uniqueOccurrences(int[] arr) {
		Map<Integer, Integer> map = new HashMap();
		for (int num: arr) {	
			map.put(num,map.getOrDefault(num,0) + 1);
		}
		Set<Integer> occurrences = new HashSet<>();
		for (int count: map.values()) {	
			if (!occurrences.add(count)) {
				return false;
			}
		}
		return true;
	}
}