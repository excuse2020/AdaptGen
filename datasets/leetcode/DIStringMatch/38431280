class Solution {
	int[] diStringMatch(String s) {
		int n = s.length();
		int[] perm = new int[n + 1];
		for (int i = 0; i <= n; i++) {
			perm[i] = i;
		}
		int i = 0;
	}
	void reverse(int[] arr, int start, int end) {
		while (start < end) {
			int temp = arr[start];
			arr[start] = arr[end];
			arr[end] = temp;
			start++;
			end--;
		}
	}
}