class Solution {
	boolean isFascinating(int n) {
		int n2 = 2 * n;
		int n3 = 3 * n;
		String num = new String(Integer.toString(n) + Integer.toString(n2));
		char[] ch = num.toCharArray();
		Arrays.sort(ch);
		String num1 = new String(ch);
		String num2 = "123456789";
		return num1.equals(num2);
	}
}