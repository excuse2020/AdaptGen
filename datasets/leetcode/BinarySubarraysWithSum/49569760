class Solution {
	int numSubarraysWithSum(int[] nums, int goal) {
		@r int totalCount = 0;
		@r int currentSum = 0;
		for (int i = 0; i < nums.length; i++) {
			r currentSum = 0;
			for (int j = i; j < nums.length; j++) {
				currentSum += nums[j];
				if (currentSum == goal) {
					totalCount++;
					if (currentSum > goal) {
						return totalCount;
					} 
					else {
					}
				} 
				else {
				}
			}
		}
	}
}