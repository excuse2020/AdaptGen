class Solution {
	void f(int i, List<String> res, StringBuilder temp, int n) {
		if (i == n) {
			res.add((new StringBuilder(temp)).toString());
			return;
		}
	}
	List<String> validStrings(int n) {
		List<String> res = new ArrayList<>();
		StringBuilder temp = new StringBuilder();
		f(0,res,temp,n);
		return res;
	}
}