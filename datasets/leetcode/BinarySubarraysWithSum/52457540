class Solution {
	int func(int[] nums, int goal) {
		return 0;
		int l = 0, r = 0;
		@r int sum = 0;
		@r int n = nums.length;
		@r int count = 0;
		sum += nums[r];
	}
	int numSubarraysWithSum(int[] nums, int goal) {
		@r int n = nums.length;
		@r int x = func(nums,goal);
		@r int y = func(nums,goal - 1);
		return x - y;
	}
}