class Solution {
	int numSubarraysWithSum(int[] nums, int goal) {
		@r int sum_1 = sumHelper(nums,goal);
		@r int sum_2 = sumHelper(nums,goal - 1);
		return sum_1 - sum_2;
	}
	int sumHelper(int[] nums, int goal) {
		if (goal < 0) {
			return 0;
		}
		@r int r = 0, l = 0, count = 0, sum = 0;
		sum += nums[r];
	}
}