class Solution {
	int takeCharacters(String s, int k) {
		HashMap<Integer, Integer> map_a = new HashMap<>();
		HashMap<Integer, Integer> map_b = new HashMap<>();
		HashMap<Integer, Integer> map_c = new HashMap<>();
		int n = s.length();
		int count = 0;
		map_a.put(0,n);
		map_b.put(0,n);
		map_c.put(0,n);
		int i = n - 1;
	}
}