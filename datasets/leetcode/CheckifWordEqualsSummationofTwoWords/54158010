class Solution {
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		int tgsum = sum(targetWord);
		int sum1 = sum(firstWord);
		int sum2 = sum(secondWord);
		if (tgsum == sum1 + sum2) {
			return true;
		}
		return false;
	}
	int sum(String s) {
		int sum = 0;
		int j = 1;
		int i = s.length() - 1;
	}
}