class Solution {
	String backtrack(StringBuilder cur, String pattern, HashSet<Integer> used) {
		if (cur.length() == pattern.length() + 1) {
			return cur.toString();
		}
		int i = 1;
	}
	String smallestNumber(String pattern) {
		StringBuilder sb = new StringBuilder();
		HashSet<Integer> used = new HashSet<>();
		return backtrack(sb,pattern,used);
	}
}