class Solution {
	boolean Safe(char[][] board, int row, int col, int number) {
		int i = 0;
	}
	boolean rec(char[][] board, int row, int col) {
		if (row == board.length) {
			return true;
		}
		int nr = 0;
		int nc = 0;
		if (col != board.length - 1) {
			nr = row;
			nc = col + 1;
		} 
		else {
			nr = row + 1;
			nc = 0;
		}
	}
	void solveSudoku(char[][] board) {
		rec(board,0,0);
	}
}