class Solution {
	int mod = 1000000007;
	int numberOfArrays(String s, int k) {
		int[] dp = new int[s.length()];
		Arrays.fill(dp,Integer.MIN_VALUE);
		return solve(s,k,0,dp);
	}
	int solve(String s, int k, int index, int[] dp) {
		if (index >= s.length()) {
			return 1;
		}
		if (dp[index] != Integer.MIN_VALUE) {
			return dp[index];
		}
	}
}