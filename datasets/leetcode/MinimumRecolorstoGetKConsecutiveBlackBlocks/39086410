class Solution {
	int min = Integer.MAX_VALUE;
	int minimumRecolors(String blocks, int k) {
		return sld(blocks,0,k,0,0,0,0);
	}
	int sld(String blocks, int indx, int k, int start, int bl, int wh, int size) {
		if (indx == blocks.length()) {
			min = Math.min(min,wh);
			return min;
		}
		min = Math.min(min,wh);
	}
}