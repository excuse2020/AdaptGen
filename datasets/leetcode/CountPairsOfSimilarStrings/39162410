class Solution {
	boolean compare(int[] c1, int[] c2) {
		for (int i = 0; i < 26; i++) {
			if (c1[i] == 0 && c2[i] != 0) {
				return false;
			}
			if (c1[i] != 0 && c2[i] == 0) {
				return false;
			}
		}
		return true;
	}
	int similarPairs(String[] words) {
		int n = words.length, c = 0;
		int i = 0;
		int[] c1 = new int[26];
		int k = 0;
	}
}