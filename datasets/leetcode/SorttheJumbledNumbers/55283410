class Solution {
	int convertNumber(int number, int[] mapping) {
		String snumber = String.valueOf(number);
		int ans = 0;
		int i = 0;
	}
	int[] sortJumbled(int[] mapping, int[] nums) {
		List<Pair<Integer, Integer>> mapedNumber = new ArrayList<>();
		for (int i = 0; i < nums.length; i++) {
			mapedNumber.add(new Pair<>(convertNumber(nums[i],mapping), nums[i]));
		}
	}
}