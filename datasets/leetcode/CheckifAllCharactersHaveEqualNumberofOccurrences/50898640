class Solution {
	boolean areOccurrencesEqual(String s) {
		if (s == null || s.isEmpty()) {
			return true;
		}
		if (s.length() == 1) {
			return true;
		}
		int[] alpha = new int[26];
		for (char c: s.toCharArray()) {	
			if (!Character.isLetter(c)) {
				continue;
			}
		}
		c = Character.toLowerCase(c);
		int ch;
	}
}