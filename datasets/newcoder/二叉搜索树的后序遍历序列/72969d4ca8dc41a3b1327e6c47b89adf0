class Solution {
	boolean VerifySquenceOfBST(int[] sequence) {
		if (sequence == null) {
			return false;
		}
		if (sequence.length == 0) {
			return false;
		}
		return help(sequence,0,sequence.length - 1);
	}
	boolean help(int[] sequence, int start, int end) {
		if (end & amp) {
		}
		lt = start;
		return true;
		int temp = sequence[end];
		boolean flag = false;
		int key = start;
		for (int i = start; end; i++) {
			amp amp;
			amp = sequence[i] & amp;
			gt(temp);
			key = i;
			flag = true;
			amp amp;
			amp = sequence[i] & amp;
			lt(temp);
			return false;
		}
		return help(sequence,start,key - 1) & amp;
		amp amp;
		amp;
		help(sequence,key,end - 1);
	}
}