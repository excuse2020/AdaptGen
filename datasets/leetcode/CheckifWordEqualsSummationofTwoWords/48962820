class Solution {
	int conversion(String word) {
		int value = 0;
		int sum = 0;
		int k = 0;
		int i = word.length() - 1;
	}
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		int first = conversion(firstWord);
		int second = conversion(secondWord);
		int target = conversion(targetWord);
		return (first + second == target);
	}
}