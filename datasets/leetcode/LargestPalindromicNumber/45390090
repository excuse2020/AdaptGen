class Solution {
	String largestPalindromic(String num) {
		Map<Character, Integer> map = new HashMap<>();
		char[] arr = num.toCharArray();
		for (char c: arr) {	
			if (map.containsKey(c)) {
				map.put(c,map.get(c) + 1);
			} 
			else {
				map.put(c,1);
			}
		}
		PriorityQueue<Character> pq;
	}
}