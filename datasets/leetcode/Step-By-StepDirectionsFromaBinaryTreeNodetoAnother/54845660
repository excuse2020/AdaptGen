class Solution {
	boolean find(TreeNode root, int val, StringBuilder sb) {
		if (root == null) {
			return false;
		}
		if (root.val == val) {
			return true;
		}
	}
	String getDirections(TreeNode root, int st, int des) {
		StringBuilder sb1 = new StringBuilder();
		StringBuilder sb2 = new StringBuilder();
		find(root,st,sb1);
		find(root,des,sb2);
		StringBuilder ans = new StringBuilder();
		String s1 = sb1.reverse().toString();
		int i = 0;
	}
}