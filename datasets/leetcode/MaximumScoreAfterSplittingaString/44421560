class Solution {
	int count(String s) {
		int c = 0;
		for (int i = 0; i < s.length(); i++) {
			c += s.charAt(i) - 48;
		}
		return c;
	}
	int maxScore(String s) {
		int count1 = count(s);
		if (count1 == 0 || count1 == s.length()) {
			return s.length() - 1;
		}
		int count0 = 0;
		int max = 0;
		int i = 0;
		char ch = s.charAt(i);
	}
}