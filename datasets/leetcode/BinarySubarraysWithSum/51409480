class Solution {
	int atMostSubArrays(int[] nums, int goal) {
		if (goal < 0) {
			return 0;
		}
		@r int sum = 0;
		@r int left = 0;
		@r int right = 0;
		@r int count = 0;
		sum += nums[right];
	}
	int numSubarraysWithSum(int[] nums, int goal) {
		return atMostSubArrays(nums,goal) - atMostSubArrays(nums,goal - 1);
	}
}