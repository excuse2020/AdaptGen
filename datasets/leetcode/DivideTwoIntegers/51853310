class Solution {
	int divide(int dividend, int divisor) {
		int c = 0;
		if (dividend == -2147483648 && divisor == -1) {
			c = 2147483647;
		} 
		else {
			c = dividend / divisor;
		}
		return c;
	}
	void main(String[] args) {
		Solution s = new Solution();
		int dividend = 10;
		int divisor = 3;
		s.divide(dividend,divisor);
	}
}