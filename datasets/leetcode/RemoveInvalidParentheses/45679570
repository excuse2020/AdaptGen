class Solution {
	Set<String> ans;
	boolean isValid(String s) {
		int left = 0;
		int i = 0;
	}
	void removeHelper(int i, int numPops, StringBuilder sb, String s) {
		if (numPops == 0) {
			String value;
			if (i != s.length()) {
				value = sb.toString() + s.substring(i,s.length());
			} 
			else {
				value = sb.toString();
			}
			if (isValid(value)) {
				ans.add(value);
			}
		}
		if (i == s.length()) {
			return;
		}
	}
	List<String> removeInvalidParentheses(String s) {
		Stack<Character> st = new Stack();
		int i = 0;
	}
}