class Solution {
	String smallestNumber(String str) {
		int n = str.length();
		char[] ans = new char[n + 1];
		for (int i = 0; i <= n; i++) {
			ans[i] = ((i + 1) + "").charAt(0);
		}
		int i = 0;
		int j = i;
	}
	void swap(char[] arr, int i, int j) {
		char temp = arr[i];
		arr[i] = arr[j];
		arr[j] = temp;
	}
}