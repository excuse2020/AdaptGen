class Solution {
	int getLucky(String s, int k) {
		String alpha = "abcdefghijklmnopqrstuvwxyz";
		int sum = 0;
		StringBuilder sb = new StringBuilder();
		for (int i = 0; i < s.length(); i++) {
			int pos = alpha.indexOf(s.charAt(i)) + 1;
			String st = pos + "";
			sb.append(st);
		}
		sum = 0;
		int i = 0;
		int num;
	}
}