class Solution {
	void reverse(int[] ch, int left, int right) {
		while (right < ch.length && left < right) {
			int h = ch[left];
			ch[left] = ch[right];
			ch[right] = h;
			left++;
			right--;
		}
	}
	String smallestNumber(String pattern) {
		int ch = new int[pattern.length() + 1];
		for (int i = 0; i < pattern.length() + 1; i++) {
			ch[i] = i + 1;
		}
		int i = 0;
		int temp = i;
	}
}