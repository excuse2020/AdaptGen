class Solution {
	int beauty(int[] arr) {
		int max = Integer.MIN_VALUE;
		int min = Integer.MAX_VALUE;
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] != 0) {
				min = Math.min(arr[i],min);
			}
			max = Math.max(arr[i],max);
		}
		return max - min;
	}
	int beautySum(String s) {
		int n = s.length();
		int sum = 0;
		int i = 0;
		int arr = new int[26];
		int j = i;
	}
}