class Solution {
	int mod = 1000000007;
	int countVowelPermutation(int n) {
		int memo = new int[n + 1][180];
		for (int i = 0; i < n + 1; i++) {
			for (int j = 0; j < 180; j++) {
				memo[i][j] = -1;
			}
		}
		long count = 0;
	}
	long helper(int size, int n, char prevChar, int dp) {
		if (size == n) {
			return 1;
		}
		if (dp[size][prevChar] != -1) {
			return dp[size][prevChar];
		}
		long ans = 0;
	}
}