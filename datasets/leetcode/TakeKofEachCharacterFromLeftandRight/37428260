class Solution {
	boolean areAllCountersFull(int[] letterCounter, int k) {
		for (int i = 0; i < 3; i++) {
			if (letterCounter[i] < k) {
				return false;
			}
		}
		return true;
	}
	int sumArray(int[] arr) {
		int sum = 0;
		for (int j: arr) {	
			sum += j;
		}
		return sum;
	}
	int takeCharacters(String s, int k) {
		if (k == 0) {
			return k;
		}
		if (s.length() < 3) {
			return -1;
		}
		int leftPivot = 0;
		int rightPivot = s.length() - 1;
		int[] letterCounter = new int[3];
		int minOperations;
	}
}