class Solution {
	int countSubArray(int nums, int goal) {
		@r int len = nums.length;
		@r int lowerIndex = 0;
		@r int higherIndex = 0;
		@r int sum = 0;
		@r int count = 0;
		if (goal < 0) {
			return 0;
		}
		sum += nums[higherIndex];
	}
	int numSubarraysWithSum(int[] nums, int goal) {
		@r int noOfSubarrayWithSum = countSubArray(nums,goal) - countSubArray(nums,goal - 1);
		return noOfSubarrayWithSum;
	}
}