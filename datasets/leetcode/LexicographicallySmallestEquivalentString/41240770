class Solution {
	void replace(int[] arr, int val, int min) {
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] == val) {
				arr[i] = min;
			}
		}
	}
	String smallestEquivalentString(String s1, String s2, String baseStr) {
		int[] arr = new int[26];
		int min = 0;
		int i = 0;
		int first;
	}
}