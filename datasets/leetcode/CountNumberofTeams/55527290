class Solution {
	int numTeams(int[] rating) {
		int count = 0;
		for (int i = 0; i < rating.length; i++) {
			int ls = 0, lg = 0, rs = 0, rg = 0;
			for (int j = 0; j < i; j++) {
				if (rating[j] < rating[i]) {
					ls++;
				} 
				else {
					lg++;
				}
			}
			for (int j = i + 1; j < rating.length; j++) {
				if (rating[j] < rating[i]) {
					rs++;
				} 
				else {
					rg++;
				}
			}
			count += ls * rg;
			count += lg * rs;
		}
		return count;
	}
}