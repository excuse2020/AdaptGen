class Solution {
	List<String> findAndReplacePattern(String[] words, String pattern) {
		List<String> matchWords = new ArrayList();
		for (String word: words) {	
			if (isIsomorphic(word,pattern)) {
				matchWords.add(word);
			}
		}
		return matchWords;
	}
	boolean isIsomorphic(String word, String pattern) {
