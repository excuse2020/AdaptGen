class Solution {
	List<String> summaryRanges(int[] nums) {
		List<String> ans = new ArrayList<>();
		if (nums.length == 0) {
			return ans;
		}
		int count = 1;
		int prevIndex = 0;
		int i;
		for (i = 1; i < nums.length; i++) {
			if (nums[i] - nums[i - 1] == 1) {
				count++;
				continue;
			} 
			else {
				if (count == 1) {
					ans.add(nums[prevIndex] + "");
				} 
				else {
					ans.add(nums[prevIndex] + "->");
				}
				prevIndex = i;
				count = 1;
			}
		}
		if (count == 1) {
			ans.add(nums[prevIndex] + "");
		} 
		else {
			ans.add(nums[prevIndex] + "->");
		}
		return ans;
	}
}