class Solution {
	int[][] spiralMatrix(int m, int n, ListNode head) {
		int[][] ans = new int[m][n];
		for (int i = 0; i < m; i++) {
			for (int j = 0; j < n; j++) {
				ans[i][j] = -1;
			}
		}
		int x = 0;
		int y = 0;
		int dir = 1;
		boolean axis = true;
		ans[y][x] = head.val;
		if (x + dir == n || x + dir == -1 || ans[y][x + dir] != -1) {
			axis = false;
			y += dir;
		} 
		else {
			x += dir;
		}
		@defines when we;
	}
}