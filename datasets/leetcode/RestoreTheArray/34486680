class Solution {
	int MOD = (int) (1e9 + 7);
	Integer dp;
	int numberOfArrays(String s, int k) {
		dp = new Integer[s.length()];
		char array = s.toCharArray();
		return solve(array,k,0);
	}
	int solve(char[] array, int k, int i) {
		if (i == array.length) {
			return 1;
		}
		if (dp[i] != null) {
			return dp[i];
		}
		long num = 0;
		long ans = 0;
		int j = i;
	}
}