class Solution {
	int minOperations(int n) {
		int numOperations = 0;
		String binaryFormForN = Integer.toBinaryString(n);
		int binaryIndex = 0;
		boolean didOperation = false;
		int i = binaryFormForN.length() - 1;
		char c1 = binaryFormForN.charAt(i);
		char c2 = binaryFormForN.charAt(i - 1);
	}
}