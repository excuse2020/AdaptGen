class Solution {
	int Helper(int[] nums, int goal) {
		if (goal < 0) {
			return 0;
		}
		@r int i = 0;
		@r int j = 0;
		@r int cnt = 0, sum = 0;
		@r int n = nums.length;
		while (j < nums.length) {
		}
		r sum = sum + nums[j];
		while (sum > goal) {
		}
		r sum = sum - nums[i];
		i++;
		r cnt = cnt + j - i + 1;
		j++;
		return cnt;
	}
	int numSubarraysWithSum(int[] nums, int goal) {
	}
}