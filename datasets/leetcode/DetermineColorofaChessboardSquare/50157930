class Solution {
	boolean squareIsWhite(String coordinates) {
		String s = "badcfehg";
		int index = s.indexOf(coordinates.charAt(0));
		int i = (int) coordinates.charAt(1);
		if (index % 2 == 0 && i % 2 != 0) {
			return true;
		} 
		else if (index % 2 != 0 && i % 2 == 0) {
			return true;
		}
		return false;
	}
}