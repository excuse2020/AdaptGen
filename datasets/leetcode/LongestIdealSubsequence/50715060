class Solution {
	int dp;
	int findlong(char ch, int idx, String s, int k) {
		if (idx == s.length()) {
			return 0;
		}
		int ans = 0;
		if (dp[idx][ch] != -1) {
			return dp[idx][ch];
		}
	}
	int longestIdealString(String s, int k) {
		dp = new int[s.length() + 1][256 + 1];
		for (int i = 0; i < dp.length; i++) {
			for (int j = 0; j < dp[0].length; j++) {
				dp[i][j] = -1;
			}
		}
	}
}