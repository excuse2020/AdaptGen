class Solution {
	int INF = Integer.MAX_VALUE;
	long minimumCost(String source, String target, char[] original, char[] changed, int[] cost) {
		int[][] adj = new int[26][26];
		for (int i = 0; i < 26; i++) {
			for (int j = 0; j < 26; j++) {
				if (i == j) {
					continue;
				}
				adj[i][j] = Integer.MAX_VALUE;
			}
		}
		int i = 0;
		int a;
	}
}