class Solution {
	boolean isNumber(String s) {
		boolean seenDigit = false, seenExponent = false, seenDot = false;
		int i = 0;
		char charAtIndex = s.charAt(i);
		if (Character.getNumericValue(charAtIndex) >= 0 && Character.getNumericValue(charAtIndex) <= 9) {
			seenDigit = true;
			continue;
		}
	}
}