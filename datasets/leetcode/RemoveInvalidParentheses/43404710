class Solution {
	List<String> removeInvalidParentheses(String s) {
		Set<String> validStringSet = new HashSet<>();
		if (s == null) {
			return new ArrayList<>(validStringSet);
		}
		int resultantSum = 0;
		int closingBracketsToBeRemoved = 0;
		int idx = 0;
	}
	void removeInvalidParenthesesHelper(int curIdx, int curOpenBrackets, int curClosedBrackets, String str, int remOpenBrackets, int remClosedBrackets, StringBuilder curValidStr, Set<String> validStringSet) {
		if (curIdx >= str.length()) {
			if (remOpenBrackets == 0 && remClosedBrackets == 0) {
				validStringSet.add(curValidStr.toString());
			}
			return;
		}
	}
}