class Solution {
	int canBeTypedWords(String text, String b) {
		StringBuilder sb = new StringBuilder();
		for (int i = 0; i < text.length(); i++) {
			sb.append(text.charAt(i));
		}
		char[] broken = new char[b.length()];
		String[] arr = text.split(" ");
		for (int i = 0; i < b.length(); i++) {
			broken[i] = b.charAt(i);
		}
		int i = 0;
		char ch = b.charAt(i);
		int j = 0;
	}
}