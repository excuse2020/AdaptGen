class Solution {
	long compute(String left, String right) {
		long res1;
	}
	long computeLeft(String s, char digit) {
		long ret = 0l;
		int i = s.length() - 1;
		ret += i + 1;
	}
	long computeRight(String s, char digit) {
		long ret = 0l;
		int i = 0;
		ret += (s.length() - i);
	}
	long minimumCost(String s) {
		int n = s.length();
		return compute(s.substring(0,n / 2),s.substring(n / 2,n));
	}
}