class Solution {
	char findKthBit(int n, int k) {
		String s = "0";
		if (n == 1) {
			return s.charAt(0);
		}
		for (int i = 0; i < n; i++) {
			s = s + "1";
		}
		return s.charAt(k - 1);
	}
	String ri(String s) {
		StringBuilder sb = new StringBuilder(s);
		String r = sb.reverse().toString();
		StringBuilder sb1 = new StringBuilder();
		int i = 0;
	}
}