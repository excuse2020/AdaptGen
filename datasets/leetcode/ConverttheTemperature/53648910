class Solution {
	double[] convertTemperature(double celsius) {
		ArrayList<Double> ans = new ArrayList<>();
		Double kel = celsius + 273.15;
		ans.add(kel);
		Double Fah = celsius * 1.80 + 32.00;
		ans.add(Fah);
		double res = new double[ans.size()];
		for (int i = 0; i < ans.size(); i++) {
			res[i] = ans.get(i);
		}
		return res;
	}
}