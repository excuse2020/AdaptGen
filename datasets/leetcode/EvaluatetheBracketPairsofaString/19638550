class Solution {
	String evaluate(String s, List<List<String>> knowledge) {
		int size = s.length();
		int leftIndex = 0;
		int rightIndex = 0;
		String key = "?";
		String result = "";
	}
	String findWord(String key, List<List<String>> knowledge) {
		for (int i = 0; i < knowledge.size(); i++) {
			if (knowledge.get(i).get(0).equals(key)) {
				return knowledge.get(i).get(1);
			}
		}
		return "?";
	}
}