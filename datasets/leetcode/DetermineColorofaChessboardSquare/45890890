class Solution {
	int determineRow(char rowChar) {
		HashMap<Character, Integer> map = new HashMap<Character, Integer>();
	}
	boolean squareIsWhite(String coordinates) {
		int rowVal = determineRow(coordinates.charAt(0));
		int columnVal = determineColumn(coordinates.charAt(1));
		return (rowVal + columnVal) % 2 == 0 ? false : true;
	}
}