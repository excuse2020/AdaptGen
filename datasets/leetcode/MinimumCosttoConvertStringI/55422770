class Solution {
	long minimumCost(String source, String target, char[] org, char[] cha, int[] cost) {
		int[][] alp = new int[26][26];
		for (int i = 0; i < 26; i++) {
			for (int j = 0; j < 26; j++) {
				if (i == j) {
					alp[i][j] = 0;
				} 
				else {
					alp[i][j] = Integer.MAX_VALUE;
				}
			}
		}
		int i = 0;
	}
}