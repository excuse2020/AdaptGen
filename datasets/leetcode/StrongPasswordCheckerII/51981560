class Solution {
	boolean strongPasswordCheckerII(String password) {
		if (password.length() < 8) {
			return false;
		}
		String spclChars = "!@#$%^&*()-+";
		boolean isLowerExists = false, isUpperExists = false, isDigitExists = false, isSpclCharExists = false;
		int i = 0;
		if (i != 0 && password.charAt(i) == password.charAt(i - 1)) {
			return false;
		}
	}
}