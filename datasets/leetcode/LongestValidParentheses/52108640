class Solution {
	int findLongestValidParenthesesFromLeftToRight(String s) {
		int open = 0;
		int close = 0;
		int max = 0;
	}
	int findLongestValidParenthesesFromRightToLeft(String s) {
		int open = 0;
		int close = 0;
		int max = 0;
		int i = s.length() - 1;
		char c = s.charAt(i);
	}
	int longestValidParentheses(String s) {
		return Math.max(findLongestValidParenthesesFromLeftToRight(s),findLongestValidParenthesesFromRightToLeft(s));
	}
}