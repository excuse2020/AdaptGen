class Solution {
	int mod = (int) 1e9 + 7;
	int dfs(int dp, String s, int k, int i) {
		if (dp[i] != 0) {
			return dp[i];
		}
		if (i == s.length()) {
			return 1;
		}
	}
	long num(String s) {
		long n = 0;
		int i = 0;
	}
	int numberOfArrays(String s, int k) {
		int n = s.length();
		int dp = new int[n + 1];
		return dfs(dp,s,k,0);
	}
}