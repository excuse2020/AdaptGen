class Solution {
	int gcd(int a, int b) {
		return (b == 0) ? a : gcd(b,a % b);
	}
	boolean hasGroupsSizeX(int[] deck) {
		Map<Integer, Integer> map = new HashMap<>();
		for (int i: deck) {	
			map.put(i,map.getOrDefault(i,0) + 1);
		}
		int gcd = 0;
		for (int val: map.values()) {	
			gcd = gcd(gcd,val);
		}
		return gcd > 1;
	}
}