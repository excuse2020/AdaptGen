class Solution {
	void reverse(int i, int j, int[] ans) {
		while (i < j) {
			int temp = ans[i];
			ans[i] = ans[j];
			ans[j] = temp;
			i++;
			j--;
		}
	}
	String smallestNumber(String pattern) {
		int[] ans = new int[pattern.length() + 1];
		for (int i = 1; i <= ans.length; i++) {
			ans[i - 1] = i;
		}
		char[] pat = pattern.toCharArray();
		int j = 0;
		boolean dCame = false;
		int i = 0;
		char ch = pat[i];
		j = i;
	}
}