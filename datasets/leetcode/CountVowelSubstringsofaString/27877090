class Solution {
	boolean containsAllVowels(byte[] vowels) {
		for (byte b: vowels) {	
			if (b == 0) {
				return false;
			}
		}
		return true;
	}
	int countVowelSubstrings(String word) {
		int countSubs = 0;
		byte[] vowels = new byte[5];
		int i = 0;
		int count = i;
		char c = word.charAt(count);
	}
	boolean isVowel(char c) {
	}
	void refreshArray(byte[] vowels) {
		for (int i = 0; i < vowels.length; i++) {
			vowels[i] = 0;
		}
	}
}