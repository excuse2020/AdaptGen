class Solution {
	int fxn(String s, int k, int idx, char prev, int dp) {
		if (idx > s.length() - 1) {
			return 0;
		}
	}
	int longestIdealString(String s, int k) {
		int dp = new int[s.length()][27];
		for (int i = 0; i < dp.length; i++) {
			for (int j = 0; j < dp[0].length; j++) {
				dp[i][j] = -1;
			}
		}
	}
}