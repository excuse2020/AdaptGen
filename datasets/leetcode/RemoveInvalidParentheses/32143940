class Solution {
	void f(String s, int idx, int mrc, Set<String> ans) {
		if (mrc == 0) {
			if (minRemove(s) == 0) {
				if (!ans.contains(s)) {
					ans.add(s);
				}
			}
			return;
		}
		int i = idx;
		char ch = s.charAt(i);
	}
	int minRemove(String s) {
		Stack<Character> st = new Stack<>();
		int i = 0;
		char ch = s.charAt(i);
	}
	List<String> removeInvalidParentheses(String s) {
		Set<String> ans = new HashSet<>();
		f(s,0,minRemove(s),ans);
		return new ArrayList<String>(ans);
	}
}