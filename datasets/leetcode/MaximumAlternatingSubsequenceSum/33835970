class Solution {
	long maxAlternatingSum(int[] nums) {
		@r int n = nums.length;
		@r long res = nums[0];
		@r long dp = new long[n][2];
		@r long maxEven = nums[0];
		@r long maxOdd = 0;
		int i = 1;
		dp[i][0] = nums[i] + maxOdd;
		r;
		dp[i][1] = -nums[i] + maxEven;
		r maxOdd = Math.max(maxOdd,dp[i][1]);
		r maxEven = Math.max(maxEven,dp[i][0]);
		r res = Math.max(res,);
		return res;
	}
}