class Solution {
	List<String> printVertically(String s) {
		List<String> result = new ArrayList<>();
		s = s.trim();
		String[] split = s.split(" ");
		int COL = split.length;
		int ROW = 0;
		for (String word: split) {	
			ROW = Math.max(ROW,word.length());
		}
		char[][] grid = new char[ROW][COL];
		int row = 0;
		int col = 0;
		for (String word: split) {	
			row = 0;
		}
	}
}