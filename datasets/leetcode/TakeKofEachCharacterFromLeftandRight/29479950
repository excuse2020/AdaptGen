class Solution {
	boolean check(int[] a, int k) {
		for (int ele: a) {	
			if (ele < k) {
				return false;
			}
		}
		return true;
	}
	int takeCharacters(String s, int k) {
		if (k == 0) {
			return 0;
		}
		int n = s.length();
		int req = k * 3;
		s = s + s;
		int[] f = new int[3];
		int i = 0;
		char ch = s.charAt(i);
	}
}