class Solution {
	String getEncryptedString(String s, int k) {
		int n = s.length();
		k = k % n;
		StringBuilder encryptedString = new StringBuilder();
		for (int i = 0; i < n; i++) {
			int newPosition = (i + k) % n;
			encryptedString.append(s.charAt(newPosition));
		}
		return encryptedString.toString();
	}
}