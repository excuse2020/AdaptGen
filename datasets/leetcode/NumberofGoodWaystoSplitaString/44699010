class Solution {
	int numSplits(String s) {
		int count = 0;
		char[] cArrLeft = new char[27];
		char[] cArrRight = new char[27];
		int[] posLeftArr = new int[s.length()];
		int[] posRightArr = new int[s.length()];
		int right = s.length() - 1;
		int left = 0, leftDis = 0, rightDis = 0;
		char rightC = s.charAt(right);
		char leftC = s.charAt(left);
	}
}