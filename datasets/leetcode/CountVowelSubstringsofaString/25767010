class Solution {
	int AtMostKVowels(String s, int k) {
		HashMap<Character, Integer> map = new HashMap<>();
		int n = s.length();
		int result = 0, i = 0;
		int j = 0;
		char ch = s.charAt(j);
	}
	int countVowelSubstrings(String word) {
		int five = AtMostKVowels(word,5);
		int four = AtMostKVowels(word,4);
		return (five - four);
	}
}