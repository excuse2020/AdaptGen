class Solution {
	String addBinary(String a, String b) {
		int diff = a.length() - b.length();
		diff = Math.abs(diff);
		if (a.length() > b.length()) {
			b = addzero(b,diff);
		} 
		else {
			a = addzero(a,diff);
		}
		StringBuilder sb = new StringBuilder();
		int carry = 0;
		int ai = 0;
		int bi = 0;
		int add = 0;
		int i = a.length() - 1;
		char aii = a.charAt(i);
		char bii = b.charAt(i);
	}
	String addzero(String s, int d) {
		StringBuilder sb = new StringBuilder();
	}
}