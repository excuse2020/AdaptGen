class Solution {
	int ans = 0;
	int[] dp = new int[100005];
	int mod = (int) (1e9) + 7;
	int numberOfArrays(String s, int k) {
		String p = "";
		Arrays.fill(dp,-1);
		int i = 0;
		p += s.charAt(i);
		long num = Long.parseLong(p);
	}
	int solve(int ind, String s, int k) {
		if (dp[ind] != -1) {
			return dp[ind];
		}
		String p = "";
		int ret = 0;
		int i = ind;
		p += s.charAt(i);
		long num = Long.parseLong(p);
	}
}