class Solution {
	long makeIntegerBeautiful(long n, int target) {
		if (n <= target) {
			return 0;
		}
		String nums = String.valueOf(n);
		int len = nums.length();
		long power = -1;
		long initialVal = n;
		len--;
		power++;
		long res = sod(n);
		break;
		nums = String.valueOf(n);
	}
	long sod(long n) {
		long res = 0;
		while (n > 0) {
			res += n % 10;
			n /= 10;
		}
		return res;
	}
}