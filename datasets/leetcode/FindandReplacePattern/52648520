class Solution {
	boolean check(String s1, String s2) {
		int[] a = new int[26];
		int[] b = new int[26];
		int i = 0;
	}
	List<String> findAndReplacePattern(String[] words, String pattern) {
		List<String> l = new ArrayList<>();
		for (String s: words) {	
			if (check(s,pattern)) {
				l.add(s);
			}
		}
		return l;
	}
}