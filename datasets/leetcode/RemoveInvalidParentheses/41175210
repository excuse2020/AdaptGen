class Solution {
	boolean isValid(String s) {
		int leftOpenParenthese = 0;
		int i = 0;
	}
	List<String> removeInvalidParentheses(String s) {
		Queue<String> nodes = new LinkedList<>();
		Set<String> visited = new HashSet<>();
		nodes.add(s);
		visited.add(s);
		List<String> validNodesInCurrentLayer = new ArrayList<>();
		int size = nodes.size();
		int k = 0;
		String current = nodes.poll();
		if (isValid(current)) {
			validNodesInCurrentLayer.add(current);
		}
		int i = 0;
	}
}