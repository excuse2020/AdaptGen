class Solution {
	List<String> fullJustify(String[] words, int maxWidth) {
		int arrLen = words.length;
		List<String> ans = new ArrayList<>();
		int[] spaceIndex = new int[maxWidth];
		int i = 0;
		StringBuilder sb = new StringBuilder(15);
		sb.append(words[i]);
		int spaceLim = 0;
		int currSize = words[i].length();
		i++;
	}
}