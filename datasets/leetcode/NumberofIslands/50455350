class Solution {
	boolean[][] flag;
	void incolumnchecker(char[][] grid, int row, int cols) {
		int i = row - 1;
	}
	void inrowchecker(char[][] grid, int row, int cols) {
		int i = cols - 1;
	}
	int numIslands(char[][] grid) {
		int height = grid.length;
		int width = grid[0].length;
		int n = 0;
		flag = new boolean[height][width];
		int i = 0;
		int j = 0;
	}
}