class Solution {
	int getLucky(String s, int k) {
		int i, sum = 0, j;
		String str = "", str1 = "";
		HashMap<Character, Integer> map = new HashMap<Character, Integer>();
		for (i = 0; i < 26; i++) {
			map.put((char) (i + 97),i + 1);
		}
		for (i = 0; i < s.length(); i++) {
			char ch = s.charAt(i);
			if (map.containsKey(ch)) {
				sum = (map.get(ch));
				str = str + sum;
			}
		}
		sum = 0;
		int g;
	}
}