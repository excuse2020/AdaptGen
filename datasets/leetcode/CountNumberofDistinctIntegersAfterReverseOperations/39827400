class Solution {
	int countDistinctIntegers(int[] nums) {
		int n = nums.length;
		HashSet<Integer> hs = new HashSet<>();
		for (int i: nums) {	
			first add;
			arraay items;
			the hashset;
			hs.add(i);
			int b = 0;
			while (i != 0) {
				int rem = i % 10;
				b = (b * 10) + rem;
				i = i / 10;
			}
			hs.add(b);
		}
		return hs.size();
	}
}