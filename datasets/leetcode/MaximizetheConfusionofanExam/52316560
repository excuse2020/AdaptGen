class Solution {
	int maxConsecutiveAnswers(String answerKey, int k) {
		int o1 = recOne(answerKey,k);
		int o2 = recTwo(answerKey,k);
		return Math.max(o1,o2);
	}
	int recOne(String s, int k) {
		int countF = 0;
		int l = 0;
		int r = 0;
		int n = s.length();
		int maxLen = 0;
	}
	int recTwo(String s, int k) {
		int countT = 0;
		int l = 0;
		int r = 0;
		int n = s.length();
		int maxLen = 0;
	}
}