class Solution {
	int[] dp = new int[1002];
	int F(int i, String s) {
		if (i < 0) {
			return 0;
		}
		if (dp[i] != -1) {
			return dp[i];
		}
		int[] freq = new int[26];
		int ans = 5005;
		int j = i;
	}
	int minimumSubstringsInPartition(String s) {
		Arrays.fill(dp,-1);
		return F(s.length() - 1,s);
	}
}