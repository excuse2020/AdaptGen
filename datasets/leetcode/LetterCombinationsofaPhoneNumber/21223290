class Solution {
	List<String> letterCombinations(String digits) {
		if (digits.isEmpty()) {
			ArrayList<String> ans = new ArrayList<>();
			return ans;
		}
		return phone("",digits);
	}
	List<String> phone(String p, String up) {
		if (up.isEmpty()) {
			ArrayList<String> list = new ArrayList<>();
			list.add(p);
			return list;
		}
		ArrayList<String> list = new ArrayList<>();
		int number;
	}
}