class Solution {
	int mod = (int) 1e9 + 7;
	int countVowelPermutation(int n) {
		if (n == 0) {
			return 0;
		}
		int[][] memo = new int[n + 1][26];
		for (int[] row: memo) {	
			Arrays.fill(row,-1);
		}
		HashMap<Character, char[]> map = new HashMap();
	}
	int helper(int n, char ch, HashMap<Character, char[]> map, int[][] memo) {
		if (n == 1) {
			return 1;
		}
	}
}