class Solution {
	int tempIdx = 0;
	boolean isSafeRange(int num) {
		if (num > 0 && num <= 9) {
			return true;
		}
		return false;
	}
	boolean isSafeRepeat(int[] nums, int idx, int num) {
		for (int i = idx; i >= 0; i--) {
			if (nums[i] == num) {
				tempIdx = i;
				return true;
			}
		}
		return false;
	}
	String smallestNumber(String pattern) {
		int[] arr = new int[pattern.length() + 1];
	}
}