class Solution {
	int counter;
	char[] changeOccurance(String s) {
		boolean[] visited = new boolean[s.length()];
		char[] charS = s.toCharArray();
		int nonOccurance = 0;
		int i = 0;
	}
	boolean checkForPattern(String s) {
		char[] charS = s.toCharArray();
		int i = 0;
	}
	int secondsToRemoveOccurrences(String s) {
		while (true) {
			if (checkForPattern(s)) {
				break;
			}
			s = new String(changeOccurance(s));
			counter++;
		}
		System.out.println(counter);
		return counter;
	}
}