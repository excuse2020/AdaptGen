class Solution {
	List<String> fullJustify(String[] words, int maxWidth) {
		List<String> result = new ArrayList<String>();
		List<String> mlist = new ArrayList<String>();
		StringBuilder str = new StringBuilder();
		int mlistSize = 0;
		int mlistCount = 0;
		String repeatedChar;
		int i = 0;
		mlist.add(words[i]);
		mlistSize += words[i].length();
		mlistCount++;
		int spaces = maxWidth - mlistSize;
		int theseis = mlistCount - 1;
	}
}