class Solution {
	String addStrings(String num1, String num2) {
		while (num1.length() != num2.length()) {
			if (num1.length() > num2.length()) {
				num2 = "0" + num2;
			}
			if (num2.length() > num1.length()) {
				num1 = "0" + num1;
			}
		}
		String ans = "";
		int carry = 0;
		int i = num1.length() - 1;
		char c1 = num1.charAt(i);
		char c2 = num2.charAt(i);
		int sum;
	}
}