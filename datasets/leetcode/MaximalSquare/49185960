class Solution {
	int maximalSquare(char[][] matrix) {
		if (matrix == null || matrix.length == 0 || matrix[0].length == 0) {
			return 0;
		}
		int m = matrix.length;
		int n = matrix[0].length;
		int[][] memo = new int[m][n];
		for (int[] row: memo) {	
			Arrays.fill(row,-1);
		}
		int maxSide = 0;
		int i = 0;
		int j = 0;
	}
	int maximalSquareRecursive(char[][] matrix, int i, int j, int[][] memo) {
	}
}