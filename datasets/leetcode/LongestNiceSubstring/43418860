class Solution {
	String longest = "";
	String longestNiceSubstring(String s) {
		if (s.length() < 2) {
			return "";
		}
		rec(0,s.length() - 1,s);
		return longest;
	}
	void rec(int left, int right, String s) {
		if (left >= right) {
			return;
		}
		boolean[] lower = new boolean[26];
		boolean[] upper = new boolean[26];
		int i = left;
		char c = s.charAt(i);
	}
}