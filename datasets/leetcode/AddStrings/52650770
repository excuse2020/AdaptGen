class Solution {
	String addStrings(String num1, String num2) {
		StringBuilder sb = new StringBuilder();
		char[] a = num1.toCharArray();
		int len1 = a.length - 1;
		char[] b = num2.toCharArray();
		int len2 = b.length - 1;
		int up = 0;
		int down = 0;
		if (len1 > len2) {
			up = len1;
			down = len2;
		} 
		else {
			up = len2;
			down = len1;
			b = num1.toCharArray();
			a = num2.toCharArray();
		}
		boolean carry = false;
		int carryNum = 0;
		int temp;
	}
}