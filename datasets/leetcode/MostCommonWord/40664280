class Solution {
	String mostCommonWord(String paragraph, String[] banned) {
		Set<String> bannedSet;
		if (banned != null && banned.length > 0) {
			bannedSet = new HashSet<>();
			for (String word: banned) {	
				bannedSet.add(word.toLowerCase());
			}
		} 
		else {
			bannedSet = Collections.emptySet();
		}
		Map<String, Integer> words = new HashMap<>(1000);
		int l = 0;
		int r = 0;
		int limit = paragraph.length();
		r = l;
		char c = paragraph.charAt(r);
	}
	void registerWord(Map<String, Integer> registry, String s, int l, int r) {
		String word = s.substring(l,r);
	}
}