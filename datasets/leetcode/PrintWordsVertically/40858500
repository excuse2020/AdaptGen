class Solution {
	List<String> printVertically(String sentence) {
		String[] str = sentence.split(" ");
		int len = 0;
		for (String s: str) {	
			len = Math.max(len,s.length());
		}
		List<String> res = new ArrayList<>();
		int i = 0;
		StringBuilder sbr = new StringBuilder("");
		for (String s: str) {	
			sbr.append((i >= s.length()) ? " " : s.charAt(i));
		}
	}
}