class Solution {
	List<String> list = new ArrayList<>();
	void generate(String s, int idx, int open, int close, String subs, int min) {
		if (idx == s.length()) {
			if (open == min && min == close && !list.contains(subs)) {
				list.add(subs);
			}
			return;
		}
		char ch = s.charAt(idx);
	}
	List<String> removeInvalidParentheses(String s) {
		int open = 0;
		int close = 0;
	}
}