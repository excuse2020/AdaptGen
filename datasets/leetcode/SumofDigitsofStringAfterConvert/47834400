class Solution {
	int addDigits(int num) {
		int sum = 0;
		while (num > 0) {
			int temp = num % 10;
			sum += temp;
			num /= 10;
		}
		return sum;
	}
	int getLucky(String s, int k) {
		int sum = 0;
		int i = 0;
	}
	int sumCalculation(int num, int sum) {
		while (num > 0) {
			int temp = num % 10;
			sum += temp;
			num /= 10;
		}
		return sum;
	}
}