class Solution {
	List<String> validStrings(int n) {
		List<String> result = new ArrayList<>();
		result.add("0");
		result.add("1");
		int i = 2;
		int j = 0;
		String str = result.get(j);
		StringBuilder sb = new StringBuilder(str);
		sb.append("1");
		result.remove(j);
		result.add(j,sb.toString());
	}
}