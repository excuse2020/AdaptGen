class Solution {
	int calculateInt(String s, int sign, int result, int index) {
		if (index >= s.length() || (!Character.isDigit(s.charAt(index)))) {
			return result * sign;
		}
		int digit;
	}
	int myAtoi(String s) {
		s = s.trim();
		int sign = 1;
		int index = 0;
		if (s.length() == 0) {
			return 0;
		}
	}
}