class Solution {
	List<String> printVertically(String s) {
		List<String> l = new ArrayList<String>();
		String str = s.split(" ");
		int max = str[0].length();
		for (int i = 1; i < str.length; i++) {
			if (str[i].length() > max) {
				max = str[i].length();
			}
		}
		char arr = new char[str.length][max];
		int i = 0;
		int j = 0;
	}
}