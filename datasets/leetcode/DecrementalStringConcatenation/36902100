class Solution {
	Integer[][][] dp;
	int minimizeConcatenatedLength(String[] words) {
		int n = words.length;
		dp = new Integer[n][26][26];
		String s = words[0];
	}
	int solve(String[] words, int i, int startChar, int endChar) {
		if (i >= words.length) {
			return 0;
		}
		if (dp[i][startChar][endChar] != null) {
			return dp[i][startChar][endChar];
		}
		String cur = words[i];
		int len1, len2 = 0;
	}
}