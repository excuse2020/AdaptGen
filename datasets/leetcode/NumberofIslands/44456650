class Solution {
	int count = 0;
	void helper(int i, int j, boolean[][] visited, char[][] grid) {
		if (i < 0 || i >= grid.length || j < 0 || j >= grid[0].length) {
			return;
		}
	}
	int numIslands(char[][] grid) {
		int row = grid.length, col = grid[0].length;
		boolean[][] visited = new boolean[row][col];
		int i = 0;
		int j = 0;
	}
}