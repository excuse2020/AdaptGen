class Solution {
	int nextGreaterElement(int n) {
		String s = Integer.toString(n);
		int arr = new int[s.length()];
		int i = 0;
	}
	void reverse(int[] arr, int i) {
		int j = arr.length - 1;
		while (i < j) {
			swap(arr,i,j);
			i++;
			j--;
		}
	}
	void swap(int[] arr, int i, int j) {
		int temp = arr[i];
		arr[i] = arr[j];
		arr[j] = temp;
	}
}