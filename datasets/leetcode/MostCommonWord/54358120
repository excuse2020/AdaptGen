class Solution {
	String mostCommonWord(String paragraph, String[] banned) {
		var i = 0;
		var buf = new StringBuilder();
		var temp = new HashMap<String, Integer>();
		var unique = new HashSet<String>();
		for (String s: banned) {	
			unique.add(s.toLowerCase());
		}
		String ans = null;
		int ansCount = -1;
		var current = Character.toLowerCase(paragraph.charAt(i++));
	}
}