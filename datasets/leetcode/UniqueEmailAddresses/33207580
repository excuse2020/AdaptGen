class Solution {
	String correctMail(String str) {
		StringBuilder sb = new StringBuilder();
		String s1 = "", s2 = "", s3 = "";
		int i = 0;
	}
	int numUniqueEmails(String[] emails) {
		Set<String> set = new HashSet<>();
		for (String s: emails) {	
			set.add(correctMail(s));
		}
		return set.size();
	}
}