class Solution {
	boolean evaluateTree(TreeNode root) {
		if (root == null) {
			return false;
		}
		if (root.val == 0) {
			return false;
		}
		if (root.val == 1) {
			return true;
		}
		boolean left = evaluateTree(root.left);
		boolean right = evaluateTree(root.right);
		if (root.val == 3) {
			return left & right;
		}
		if (root.val == 2) {
			return left | right;
		}
		return true;
	}
}