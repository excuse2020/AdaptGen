class Solution {
	int convertToNum(String word) {
		StringBuilder sb = new StringBuilder();
		int i = 0;
	}
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		int first = convertToNum(firstWord);
		int second = convertToNum(secondWord);
		int target = convertToNum(targetWord);
		return (first + second) == target;
	}
}