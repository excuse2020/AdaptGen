class Solution {
	int maxLength(List<String> arr) {
		return maxLengthHelper(arr,0,0,0);
	}
	int maxLengthHelper(List<String> arr, int index, int currentLength, int bitmask) {
		if (index == arr.size()) {
			return currentLength;
		}
		int maxLength = maxLengthHelper(arr,index + 1,currentLength,bitmask);
		int stringBitmask = 0;
		int charBit;
	}
}