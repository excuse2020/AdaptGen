class Solution {
	int helper(int idx, char[] arr, int[][] dp, char prev) {
		if (idx == arr.length) {
			return 0;
		}
		int prevState;
	}
	int minFlipsMonoIncr(String s) {
		char[] arr = s.toCharArray();
		int[][] dp = new int[arr.length][2];
		for (int[] row: dp) {	
			Arrays.fill(row,-1);
		}
	}
}