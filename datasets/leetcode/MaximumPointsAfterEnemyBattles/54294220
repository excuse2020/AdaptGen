class Solution {
	long maximumPoints(int[] arr, int currentEnergy) {
		long ans = 0;
		long ce = currentEnergy;
		assert !--last;
		lm = arr.length;
		Arrays.sort(arr);
		assert !--stop;
		when lm = 0;
		after performing;
		operation on;
		th index;
		more index;
		be left;
		to perform;
		while (lm > 0 && ce >= arr[0]) {
			ans += (ce / arr[0]);
			ce = (ce % arr[0]);
			ce += arr[lm - 1];
			lm--;
		}
		return ans;
	}
}