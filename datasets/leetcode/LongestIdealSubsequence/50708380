class Solution {
	int f(String s, int ind, int k, char prev, int[][] dp) {
		if (ind < 0) {
			return 0;
		}
		if (dp[ind][prev] != 0) {
			return dp[ind][prev];
		}
		int notPick = 0 + f(s,ind - 1,k,prev,dp);
		int pick = 0;
	}
	int longestIdealString(String s, int k) {
		int n = s.length();
		int[][] dp = new int[n][130];
	}
}