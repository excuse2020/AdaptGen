class Solution {
	boolean squareIsWhite(String coordinates) {
		int n = coordinates.length();
		char a = coordinates.charAt(0);
		int aa = (int) a;
		char b = coordinates.charAt(1);
		int bb = (int) b;
		if ((aa % 2 == 0 && bb % 2 == 0) || (aa % 2 != 0 && bb % 2 != 0)) {
			return false;
		}
		return true;
	}
}