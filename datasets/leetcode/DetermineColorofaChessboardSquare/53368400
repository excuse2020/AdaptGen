class Solution {
	boolean squareIsWhite(String coordinates) {
		char ch = coordinates.toCharArray();
		if ((int) ch[0] % 2 == 0 && (Character.getNumericValue(ch[1])) % 2 != 0) {
			return true;
		}
		if ((int) ch[0] % 2 != 0 && (Character.getNumericValue(ch[1])) % 2 == 0) {
			return true;
		}
		return false;
	}
}