class Solution {
	int[][] t;
	int minFlipsMonoIncr(String s) {
		t = new int[2][s.length()];
		for (int[] i: t) {	
			Arrays.fill(i,-1);
		}
	}
	int solve(String s, int i, boolean isZero, int prev) {
		if (i >= s.length()) {
			return 0;
		}
		int left = 0;
		int right = 0;
		if (t[prev][i] != -1) {
			return t[prev][i];
		}
	}
}