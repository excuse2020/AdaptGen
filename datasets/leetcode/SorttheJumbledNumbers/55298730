class Solution {
	int getMappedValue(int num, int[] mapping) {
		StringBuilder mappedValue = new StringBuilder();
	}
	int[] sortJumbled(int[] mapping, int[] nums) {
		int n = nums.length;
		int[][] mappedNums = new int[n][2];
		for (int i = 0; i < n; i++) {
			mappedNums[i][0] = getMappedValue(nums[i],mapping);
			mappedNums[i][1] = i;
		}
	}
}