class Solution {
	String evaluate(String s, List<List<String>> knowledge) {
		StringBuilder sb = new StringBuilder(s);
		HashMap<String, String> hm = new HashMap<String, String>();
		for (List<String> ls: knowledge) {	
			hm.put(ls.get(0),ls.get(1));
		}
		int pointone = 0;
		int pointwo = 0;
		int index = 0;
	}
}