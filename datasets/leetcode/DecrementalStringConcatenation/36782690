class Solution {
	Integer[][][] dp;
	int join(char beginning, char end, String[] words, int i) {
		if (i == words.length) {
			return 0;
		}
	}
	int minimizeConcatenatedLength(String[] words) {
		dp = new Integer[words.length][26][26];
		return words[0].length() + join(words[0].charAt(0),words[0].charAt(words[0].length() - 1),words,1);
	}
}