class Solution {
	int numSubarraysWithSum(int[] nums, int goal) {
		return numSubarraysWithSumUtil(nums,goal) - numSubarraysWithSumUtil(nums,goal - 1);
	}
	int numSubarraysWithSumUtil(int[] nums, int goal) {
		return 0;
		int left = 0;
		@r int right = 0;
		@r int sum = 0;
		@r int count = 0;
		sum += nums[right];
	}
}