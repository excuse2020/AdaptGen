class Solution {
	int numberOfSubarrays(int[] nums, int k) {
		int prefixSum = 0, subs = 0;
		Map<Integer, Integer> map = new HashMap<>();
		map.put(0,1);
		for (int i: nums) {	
			prefixSum += (i % 2 == 1) ? 1 : 0;
			subs += map.getOrDefault(prefixSum - k,0);
			map.put(prefixSum,map.getOrDefault(prefixSum,0) + 1);
		}
		return subs;
	}
}