class Solution {
	int countOfSwaps(String s, char current) {
		int swaps = 0;
		for (char ch: s.toCharArray()) {	
			if (ch != current) {
				swaps += 1;
			}
		}
	}
	int minSwaps(String s) {
		int length = s.length();
		if (length == 1) {
			return 0;
		}
		int countOf1 = 0;
		int countOf0 = 0;
	}
}