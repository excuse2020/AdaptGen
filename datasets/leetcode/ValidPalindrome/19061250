class Solution {
	boolean isPalindrome(String s) {
		StringBuilder v = new StringBuilder();
		int i = 0;
	}
	boolean sachMePalindromeHai(String s) {
		int survat = 0;
		int khatam = s.length() - 1;
		while (survat <= khatam) {
			if (s.charAt(survat) != s.charAt(khatam)) {
				return false;
			}
			survat++;
			khatam--;
		}
		return true;
	}
}