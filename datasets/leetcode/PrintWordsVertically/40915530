class Solution {
	List<String> printVertically(String s) {
		String[] arr = s.split(" ");
		ArrayList<String> list = new ArrayList<>();
		int max = 0;
		for (int i = 0; i < arr.length; i++) {
			max = Math.max(arr[i].length(),max);
		}
		int i = 0;
		String save = "";
		for (int j = 0; j < arr.length; j++) {
			if (arr[j].length() > i) {
				save += arr[j].charAt(i);
			} 
			else {
				save += " ";
			}
		}
		String temp = "";
		int t = 0;
		int a = save.length() - 1;
	}
}