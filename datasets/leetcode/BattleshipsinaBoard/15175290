class Solution {
	int countBattleships(char[][] board) {
		if (board == null) {
			throw new IllegalArgumentException("Input is null")
		}
		if (board.length == 0 || board[0].length == 0) {
			return 0;
		}
		int rows = board.length;
		int cols = board[0].length;
		int count = 0;
		int i = 0;
		int j = 0;
	}
}