class Solution {
	int cols;
	int islands = 0;
	int rows;
	void bfs(char[][] grid, int row, int col) {
	}
	void check(char[][] grid, int row) {
		char[] finalRow = grid[row];
		int col = 0;
	}
	int numIslands(char[][] grid) {
		rows = grid.length;
		cols = grid[0].length;
		for (int row = 0; row < rows; row++) {
			check(grid,row);
		}
		return islands;
	}
}