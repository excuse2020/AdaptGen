class Solution {
	int numSubarraysWithSum(int[] nums, int goal) {
		@r int count = 0;
		@r int sum = 0;
		for (int i = 0; i < nums.length; i++) {
			r sum = 0;
			for (int j = i; j < nums.length; j++) {
				r sum = sum + nums[j];
				if (sum == goal) {
					count++;
					return count;
				} 
				else {
				}
			}
		}
	}
}