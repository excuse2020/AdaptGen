class Solution {
	int numSubarraysWithSum(int[] nums, int goal) {
		@r int n = nums.length;
		@r int f = 0;
		for (int i = 0; i < n; i++) {
			@r int sum = 0;
			for (int j = i; j < n; j++) {
				r sum = sum + nums[j];
				if (sum == goal) {
					f++;
					if (sum > goal) {
						return f;
					} 
					else {
					}
				} 
				else {
				}
			}
		}
	}
}