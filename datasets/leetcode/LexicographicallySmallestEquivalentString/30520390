	String smallestEquivalentString(String s1, String s2, String baseStr) {
		int m = s1.length();
		int n = baseStr.length();
		ArrayList<Character>[] graph = new ArrayList[26];
		for (int i = 0; i < 26; i++) {
			graph[i] = new ArrayList<Character>();
		}
		int i = 0;
		char u = s1.charAt(i);
		char v = s2.charAt(i);
	}
}