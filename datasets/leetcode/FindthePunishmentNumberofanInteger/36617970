class Solution {
	int ans;
	int dp;
	boolean helper(String num, int target, int idx) {
		if (idx >= num.length()) {
			return target == 0;
		}
		if (target < 0) {
			return false;
		}
		if (dp[idx][target] != -1) {
			return dp[idx][target] == 1 ? true : false;
		}
		int sum = 0;
		int i = idx;
	}
	int punishmentNumber(int n) {
		return ans[n];
	}
}