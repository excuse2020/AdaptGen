class Solution {
	String shortestPalindrome(String s) {
		if (s.length() == 0) {
			return "";
		}
		long B = 27, MOD = 1000000007, hash1 = 0, hash2 = 0, POW = 1;
		int idx = 0;
		StringBuilder str = new StringBuilder(s);
		StringBuilder sb = new StringBuilder(s);
		String rev = sb.reverse().toString();
		int i = 0;
		int curr;
	}
}