class Solution {
	int maximumValue(String[] strs) {
		ArrayList<Integer> a = new ArrayList<>();
		for (int i = 0; i < strs.length; i++) {
			if (onlydigit(strs[i])) {
				a.add(Integer.parseInt(strs[i]));
			} 
			else {
				a.add(strs[i].length());
			}
		}
		return Collections.max(a);
	}
	boolean onlydigit(String s) {
		int i = 0;
	}
}