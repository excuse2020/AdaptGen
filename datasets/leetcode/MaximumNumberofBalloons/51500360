class Solution {
	int maxNumberOfBalloons(String text) {
		Map<Character, Integer> hm = new HashMap<>();
		for (int i = 0; i < text.length(); i++) {
			char ch = text.charAt(i);
			hm.put(ch,hm.getOrDefault(ch,0) + 1);
		}
		String str = "balloon";
		int min = Integer.MAX_VALUE;
		int i = 0;
		char ch = str.charAt(i);
	}
}