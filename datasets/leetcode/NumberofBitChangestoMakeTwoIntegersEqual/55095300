class Solution {
	int minChanges(int n, int k) {
		if (k > n) {
			return -1;
		}
		int count = 0;
		StringBuilder strn = new StringBuilder(Integer.toBinaryString(n));
		StringBuilder strk = new StringBuilder(Integer.toBinaryString(k));
		int i = strn.length() - 1, j = strk.length() - 1;
		i--;
		j--;
	}
}