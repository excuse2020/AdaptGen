class Solution {
	ArrayList<Integer> GetLeastNumbers_Solution(int[] input, int k) {
		ArrayList<Integer> res = new ArrayList<>();
		if (input == null || input.length < 1 || input.length < k) {
			return res;
		}
		if (k < 1) {
			return res;
		}
		int n = input.length;
		PriorityQueue<Integer> pq;
	}
}