class Solution {
	char[] a;
	char[] ans;
	boolean[] flag;
	boolean found;
	void backtracking(int po) {
		if (po == ans.length) {
			found = true;
			return;
		}
		int i;
	}
	String smallestNumber(String pattern) {
		a = pattern.toCharArray();
		ans = new char[a.length + 1];
		flag = new boolean[140];
		found = false;
		backtracking(0);
		return new String(ans);
	}
}