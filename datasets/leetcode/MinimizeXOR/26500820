class Solution {
	int minimizeXor(int num1, int num2) {
		StringBuilder str = new StringBuilder(Integer.toBinaryString(num1));
		str.reverse();
		StringBuilder str2 = new StringBuilder(Integer.toBinaryString(num2));
		str2.reverse();
		char[] binNum1 = (str.toString()).toCharArray();
		char[] binNum2 = (str2.toString()).toCharArray();
		int num1set = Integer.bitCount(num1);
		int num2set = Integer.bitCount(num2);
		char[] binX = new char[Math.max(binNum1.length,binNum2.length)];
	}
}