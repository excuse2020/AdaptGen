class Solution {
	int maxAscendingSum(int[] nums) {
		int max = nums[0];
		int curr = nums[0];
		for (int i = 1; i < nums.length; i++) {
			if (nums[i] <= nums[i - 1]) {
				max = max < curr ? curr : max;
				curr = nums[i];
			} 
			else {
				curr += nums[i];
			}
		}
		max = max < curr ? curr : max;
		return max;
	}
}