class Solution {
	int StringtoInt(String word) {
		if (word == null || word.length() == 0) {
			return 0;
		}
		Map<Character, Integer> map = new HashMap<>();
	}
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		int a = StringtoInt(firstWord);
		int b = StringtoInt(secondWord);
		int c = StringtoInt(targetWord);
		if (a + b == c) {
			return true;
		}
		return false;
	}
}