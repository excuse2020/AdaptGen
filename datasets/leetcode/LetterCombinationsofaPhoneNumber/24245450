class Solution {
	List<String> letterCombinations(String digits) {
		List<String> ans = new ArrayList<>();
		if (digits.length() == 0) {
			return ans;
		}
		theans("",digits,ans);
		return ans;
	}
	void theans(String up, String p, List<String> ans) {
		if (p.length() == 0) {
			ans.add(up);
			return;
		}
		int digit;
	}
}