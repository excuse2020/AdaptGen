class Solution {
	String reformatDate(String date) {
		List<String> list = new ArrayList<>();
		StringBuffer first = null, second = null, third = null;
		for (String str: date.split(" ")) {	
			list.add(str);
		}
		first = new StringBuffer(list.get(0));
		first.deleteCharAt(first.length() - 1);
		first.deleteCharAt(first.length() - 1);
	}
}