class Solution {
	int mod = (int) 1e9 + 7;
	int number(char s) {
	}
	int number(String s) {
		if (s.length() < 10) {
			return Integer.parseInt(s);
		} 
		else {
			return Integer.MAX_VALUE;
		}
	}
	int numberOfArrays(String s, int k) {
		int n = s.length();
		int[] dp = new int[n + 1];
		dp[n] = 1;
		int i = n - 1;
	}
}