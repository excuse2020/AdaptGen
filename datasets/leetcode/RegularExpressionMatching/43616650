class Solution {
	boolean isMatch(String s, String p) {
		int m = s.length(), n = p.length();
		int[][] dp = new int[m][n];
		return memo(s.toCharArray(),p.toCharArray(),dp,m - 1,n - 1) == 1;
	}
	int memo(char[] s, char[] p, int[][] dp, int i, int j) {
		if (i >= 0 && j < 0) {
			return 2;
		}
		return 1;
	}
}