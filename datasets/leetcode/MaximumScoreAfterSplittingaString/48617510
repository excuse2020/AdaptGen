class Solution {
	int maxScore(String s) {
		String str1 = "";
		int max = 0;
		int i = 0;
		String str2 = "";
		int j = i + 1;
		if (j == i + 1) {
			str1 = str1 + s.charAt(i);
		}
		str2 = str2 + s.charAt(j);
		System.out.println("left : " + str1);
		System.out.println("right : " + str2);
		int acount = 0;
		int a = 0;
	}
}