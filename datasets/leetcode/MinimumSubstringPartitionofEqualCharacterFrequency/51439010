class Solution {
	int[] dp;
	int f(int i, String s, int[] fre) {
		if (i >= s.length()) {
			return 0;
		}
		if (dp[i] != -1) {
			return dp[i];
		}
		int an = Integer.MAX_VALUE;
		int ind = i;
		int temp;
	}
	int minimumSubstringsInPartition(String s) {
		dp = new int[s.length()];
		Arrays.fill(dp,-1);
		int[] fre = new int[26];
		return f(0,s,fre);
	}
}