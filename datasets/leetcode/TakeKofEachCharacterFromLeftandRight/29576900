class Solution {
	boolean isValid(int cnt1, int[] cnt2, int k) {
		for (int i = 0; i < 3; i++) {
			if (cnt2[i] > cnt1[i] - k) {
				return false;
			}
		}
		return true;
	}
	int takeCharacters(String s, int k) {
		int cnt1 = new int[3];
		int cnt2 = new int[3];
		int n = s.length();
		int i = 0;
	}
}