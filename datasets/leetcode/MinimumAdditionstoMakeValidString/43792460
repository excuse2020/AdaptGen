class Solution {
	int addMinimum(String word) {
		int right = word.length() - 1;
		int result;
	}
	int calculate(char a, char b) {
		int result = 0;
		int difference = -1 * (value(a) - value(b));
		if (difference == 0) {
			result = 2;
		} 
		else if (difference == 1) {
			result = 0;
		} 
		else if (difference == 2) {
			result = 1;
		} 
		else if (difference == -1) {
			result = 1;
		} 
		else if (difference == -2) {
			result = 0;
		}
		return result;
	}
	int value(char c) {
	}
}