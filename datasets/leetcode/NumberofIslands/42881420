class Solution {
	void dfs(char arr, int i, int j, int visited) {
		if (i < 0 || j < 0 || i >= arr.length || j >= arr[0].length) {
			return;
		}
	}
	int numIslands(char[][] arr) {
		int ans = 0;
		int n = arr.length;
		int m = arr[0].length;
		int visited = new int[n][m];
		for (var a: visited) {	
			Arrays.fill(a,-1);
		}
		int i = 0;
		int j = 0;
	}
}