class Solution {
	int[] constructArray(int n, int k) {
		return util1(n,k);
	}
	int[] util1(int n, int k) {
		int left = 1, right = n;
		int ans = new int[n];
		int a = 0;
		while (k != 1) {
			if (a % 2 == 0) {
				ans[a] = right;
				right--;
			} 
			else {
				ans[a] = left;
				left++;
			}
			a++;
			k--;
		}
		if (a % 2 == 0) {
			while (right >= left) {
				ans[a] = right;
				right--;
				a++;
			}
		} 
		else {
			while (right >= left) {
				ans[a] = left;
				left++;
				a++;
			}
		}
		return ans;
	}
}