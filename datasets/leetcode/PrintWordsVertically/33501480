class Solution {
	List<String> printVertically(String s) {
		String[] str = s.split(" ");
		List<String> list = new ArrayList<>();
		int max = 0;
		for (int i = 0; i < str.length; i++) {
			max = Math.max(str[i].length(),max);
		}
		int i = 0;
		StringBuilder sc = new StringBuilder();
		int j = 0;
		sc.append(str[j].charAt(i));
	}
}