class Solution {
	String reformatDate(String date) {
		List<String> constMonths = new ArrayList<>(List.of("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"));
		StringBuilder sb = new StringBuilder();
		String[] splitedRes = date.split(" ");
		String year = splitedRes[2];
		String month = splitedRes[1];
		String day = splitedRes[0];
		sb.append(year + "-");
	}
}