class Solution {
	List<String> printVertically(String s) {
		List<String> list = new ArrayList<>();
		String[] w = s.split(" ");
		int max = 0;
		for (int i = 0; i < w.length; i++) {
			max = Math.max(max,w[i].length());
		}
		int k = 0;
		String str = w[k];
		int i = 0;
		String ss = "";
		char ch;
	}
}