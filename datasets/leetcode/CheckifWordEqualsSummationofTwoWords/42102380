class Solution {
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		int sumOfFirstWord = wordToNumber(firstWord);
		int sumOfSecondWord = wordToNumber(secondWord);
		int sumOfTargetWord = wordToNumber(targetWord);
		return (sumOfFirstWord + sumOfSecondWord) == sumOfTargetWord;
	}
