class Solution {
	boolean isBalloonPossible(int[] counts) {
		if (counts[0] > 0 && counts[1] > 0 && counts[11] > 1 && counts[13] > 0 && counts[14] > 1) {
			counts[0]--;
			counts[1]--;
			counts[11] -= 2;
			counts[13]--;
			counts[14] -= 2;
			return true;
		}
		return false;
	}
	int maxNumberOfBalloons(String text) {
		if (text.length() < 7) {
			return 0;
		}
		int[] charCount = new int[26];
	}
}