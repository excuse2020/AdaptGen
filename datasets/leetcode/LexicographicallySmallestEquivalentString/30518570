class Solution {
	int find(int[] graph, int index) {
		while (graph[index] != index) {
			index = graph[index];
		}
		return index;
	}
	String smallestEquivalentString(String s1, String s2, String baseStr) {
		int[] graph = new int[26];
		for (int i = 0; i < 26; i++) {
			graph[i] = i;
		}
		int i = 0;
		int first;
	}
}