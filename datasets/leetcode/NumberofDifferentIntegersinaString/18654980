class Solution {
	int numDifferentIntegers(String word) {
		String paddedWord = word + "a";
		StringBuilder numberStb = new StringBuilder();
		Set<String> set = new HashSet();
		int len = paddedWord.length();
		int i = 0;
		char ch = paddedWord.charAt(i);
		if (numberStb.length() > 0) {
			set.add(numberStb.toString());
			numberStb.setLength(0);
		}
	}
}