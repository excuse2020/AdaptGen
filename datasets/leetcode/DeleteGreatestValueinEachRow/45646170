class Solution {
	int deleteGreatestValue(int[][] grid) {
		for (int i = 0; i < grid.length; i++) {
			Arrays.sort(grid[i]);
		}
		int sum = 0;
		for (int j = 0; j < grid[0].length; j++) {
			int max = grid[0][j];
			for (int i = 0; i < grid.length; i++) {
				if (grid[i][j] > max) {
					max = grid[i][j];
				}
			}
			sum = sum + max;
		}
		return sum;
	}
}