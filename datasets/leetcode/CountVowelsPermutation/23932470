class Solution {
	Map<Character, Integer> charMap = new HashMap<>();
	int[][] memo;
	int countVowelPermutation(int n) {
		memo = new int[n + 1][5];
		long answer;
	}
	int countVowelPermutation(int n, char prevChar) {
		if (n == 0) {
			return 1;
		}
		int charIndex = charMap.get(prevChar);
		if (memo[n][charIndex] != 0) {
			return memo[n][charIndex];
		}
		long answer = 0;
	}
}