class Solution {
	int n;
	int result = 0;
	void findMaxLenght(String answerKey) {
		int length = 0;
		int i = 0;
	}
	void flip(String answerKey, int k, int i) {
		findMaxLenght(answerKey);
		if (i >= n || k == 0) {
			return;
		}
	}
	int maxConsecutiveAnswers(String answerKey, int k) {
		n = answerKey.length();
		flip(answerKey,k,0);
		return result;
	}
}