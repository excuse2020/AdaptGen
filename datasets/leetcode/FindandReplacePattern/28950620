class Solution {
	List<String> findAndReplacePattern(String[] words, String pattern) {
		List<String> ans = new ArrayList<>();
		for (int i = 0; i < words.length; i++) {
			String s = words[i];
			if (solve(s,pattern)) {
				ans.add(s);
			}
		}
		return ans;
	}
	boolean solve(String w, String p) {
		int[] wi = new int[26], pi = new int[26];
		int l = 0;
	}
}