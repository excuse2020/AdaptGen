class Solution {
	String mostCommonWord(String paragraph, String[] banned) {
		Set<String> bannedSet = Set.of(banned);
		Map<String, Integer> map = new HashMap<>();
		String temp = null;
		int max = 0, curr = 0;
		String[] words = paragraph.split("[ ,]+");
		for (String word: words) {	
			word = word.trim().toLowerCase();
		}
	}
}