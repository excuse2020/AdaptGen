class Solution {
	boolean strongPasswordCheckerII(String password) {
		if (password.length() < 8) {
			return false;
		}
		boolean res = false;
		boolean lc = false;
		boolean uc = false;
		boolean d = false;
		boolean sc = false;
		int i = 0;
		char ch = password.charAt(i);
		if (i > 0 && ch == password.charAt(i - 1)) {
			return false;
		}
	}
}