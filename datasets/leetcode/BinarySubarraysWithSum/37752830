class Solution {
	int numSubarraysWithSum(int[] nums, int goal) {
		@r int x = solve(nums,goal);
		@r int y = solve(nums,goal - 1);
		return x - y;
	}
	int solve(int[] nums, int k) {
		return 0;
		int s = 0;
		@r int e = 0;
		@r int sum = 0;
		@r int ans = 0;
		sum += nums[e];
		sum -= nums[s];
		s++;
	}
}