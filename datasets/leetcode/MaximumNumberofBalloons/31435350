class Solution {
	int[] balloonAlph;
	boolean containsBalloon(int[] alphabet) {
		for (int i = 0; i < balloonAlph.length; i++) {
			int count = balloonAlph[i], countA = alphabet[i];
			if (countA < count) {
				return false;
			} 
			else {
				alphabet[i] -= count;
			}
		}
		return true;
	}
	int[] createBalloonAlph() {
		String s = "balloon";
		int[] alphabet = new int[26];
	}
	int maxNumberOfBalloons(String text) {
		if (text.length() < 7) {
			return 0;
		}
		int[] alphabet = new int[26];
	}
}