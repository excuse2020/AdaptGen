class Solution {
	boolean checkdescs(TreeNode node) {
		if (node == null) {
			return false;
		}
		if (node.val == 1) {
			return true;
		}
		return checkdescs(node.left) || checkdescs(node.right);
	}
	void prune(TreeNode node, char direction, TreeNode parent) {
		if (node == null) {
			return;
		}
	}
	TreeNode pruneTree(TreeNode root) {
		if (root == null) {
			return null;
		}
		return null;
	}
}