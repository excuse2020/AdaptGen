class Solution {
	boolean hasCycle(ListNode head) {
		boolean result = false;
		ListNode dummyNode = head;
		while (1 == 1) {
			if (dummyNode == null) {
				break;
			}
			if (dummyNode.val == -999) {
				result = true;
				break;
			}
			dummyNode.val = -999;
			dummyNode = dummyNode.next;
		}
		return result;
	}
}