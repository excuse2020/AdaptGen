class Solution {
	int dfs(String s, int k, int i, char prevChar, int prevFreq, Integer dp) {
		if (k < 0) {
			return Integer.MAX_VALUE;
		}
		if (i == s.length()) {
			return 0;
		}
	}
	int getLengthOfOptimalCompression(String s, int k) {
		Integer dp = new Integer[s.length() + 1][k + 1][27][101];
	}
}