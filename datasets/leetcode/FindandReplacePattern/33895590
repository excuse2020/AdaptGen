class Solution {
	boolean check(String s, String p) {
		int[] bij = new int[26];
		boolean[] vis = new boolean[26];
		Arrays.fill(bij,-1);
		int i = 0;
		int key;
	}
	List<String> findAndReplacePattern(String[] words, String pattern) {
		List<String> res = new ArrayList<>();
		for (String s: words) {	
			if (check(s,pattern)) {
				res.add(s);
			}
		}
		return res;
	}
}