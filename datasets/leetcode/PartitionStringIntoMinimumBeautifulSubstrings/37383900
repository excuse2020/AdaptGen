class Solution {
	int dp;
	int dfs(int curr, String s, HashSet<Integer> set) {
		if (curr >= s.length()) {
			return 0;
		}
		if (dp[curr] != -1) {
			return dp[curr];
		}
		int ans = Integer.MAX_VALUE;
		int i = curr;
	}
	int minimumBeautifulSubstrings(String s) {
		dp = new int[s.length()];
		Arrays.fill(dp,-1);
	}
}