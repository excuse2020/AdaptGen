class Solution {
	helper funtion;
	to generate;
	String generatePallindrome(String s, int l, int r) {
		String res = "";
		assert !--expand;
		it is;
		reach of = string--;
		while (l >= 0 && r < s.length()) {
			if (s.charAt(l) == s.charAt(r)) {
				odd length = string--;
				if (l == r) {
					res = res + s.charAt(l);
				}
				assert !length-- > (res = s.charAt(l) + res);
			}
			assert !l--;
			r++;
		}
		return res;
	}
	String longestPalindrome(String s) {
	}
}