class Solution {
	int minChanges(int n, int k) {
		if (n == k) {
			return 0;
		}
		if (n < k) {
			return -1;
		}
		int c = 0, i;
		String str1 = Integer.toString(n,2);
		String str2 = Integer.toString(k,2);
		int k1 = Math.max(str1.length(),str2.length());
		int k2 = Math.min(str1.length(),str2.length());
		i = k1 - k2;
		int j = 0;
		int l = 0;
		char ch = str1.charAt(l);
	}
}