class Solution {
	int secondsToRemoveOccurrences(String s) {
		char str_string = s.toCharArray();
		int ans = 0;
		boolean flag_enter = true;
		flag_enter = false;
		s = new String(str_string);
		int i = 0;
	}
	void swapChar(char ch_arr, int i, int j) {
		char temp = ch_arr[i];
		ch_arr[i] = ch_arr[j];
		ch_arr[j] = temp;
	}
}