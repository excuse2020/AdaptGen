class Solution {
	int getNumericalValue(String str) {
		int value = 0;
		int digit;
	}
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		int firstWordValue = getNumericalValue(firstWord);
		int secondWordValue = getNumericalValue(secondWord);
		int targetWordValue = getNumericalValue(targetWord);
		return (firstWordValue + secondWordValue) == targetWordValue;
	}
}