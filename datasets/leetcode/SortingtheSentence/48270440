class Solution {
	String ans(String s) {
		String[] a = s.split(" ");
		String[] ans = s.split(" ");
		int i = 0;
		int b;
	}
	String convert(String[] ans, String delimeter) {
		StringBuilder sb = new StringBuilder();
		for (String i: ans) {	
			sb.append(i).append(delimeter);
		}
		return sb.substring(0,sb.length() - 1);
	}
	String sortSentence(String s) {
		return ans(s);
	}
}