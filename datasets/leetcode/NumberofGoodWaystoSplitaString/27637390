class Solution {
	boolean isGoodSplit(int[] left, int[] right) {
		int lc = 0, rc = 0;
		for (int i = 0; i < 26; i++) {
			if (left[i] > 0) {
				lc++;
			}
			if (right[i] > 0) {
				rc++;
			}
		}
		return lc == rc;
	}
	int numSplits(String s) {
		int[] left = new int[26], right = new int[26];
	}
}