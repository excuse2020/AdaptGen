class Solution {
	String decToBinary(int n) {
		StringBuilder sb = new StringBuilder();
		int i = 31;
		int k = n >> i;
	}
	int minChanges(int n, int k) {
		String str1 = decToBinary(n);
		String str2 = decToBinary(k);
		int cnt = 0;
		int i = 0;
		char ch1 = str1.charAt(i);
		char ch2 = str2.charAt(i);
	}
}