		int length = brokenLetters.length();
		boolean[] no = new boolean[26];
		int count = 0;
		for (int i = 0; i < length; i++) {
			no[brokenLetters.charAt(i) % 26] = true;
		}
		length = text.length();
		int i = 0;
		char curr = text.charAt(i);
	}
	int check(int i, String text, int length) {
	}
}