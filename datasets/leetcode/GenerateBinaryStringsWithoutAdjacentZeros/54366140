class Solution {
	void backtrack(List<String> ret, StringBuilder cur, int n) {
		if (n == 0) {
			ret.add(cur.toString());
			return;
		}
	}
	List<String> validStrings(int n) {
		List<String> ret = new ArrayList<String>();
		StringBuilder cur = new StringBuilder();
		backtrack(ret,cur,n);
		return ret;
	}
}