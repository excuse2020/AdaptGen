class Solution {
	boolean strongPasswordCheckerII(String password) {
		if (password.length() < 8) {
			return false;
		}
		String specialChars = "!@#$%^&*()-+";
		boolean lowercase = false, uppercase = false, digit = false, special = false, adjacent = true;
		int i = 0;
		char ch = password.charAt(i);
		if (i < password.length() - 1 && ch == password.charAt(i + 1)) {
			return false;
		}
	}
}