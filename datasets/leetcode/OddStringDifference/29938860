class Solution {
	boolean equalArrays(int[] a, int[] b) {
		for (int i = 0; i < a.length; i++) {
			if (a[i] != b[i]) {
				return false;
			}
		}
		return true;
	}
	String oddString(String[] words) {
		int wordLength = words[0].length();
		int[] firstWordDiff = new int[wordLength - 1];
		int[] secondWordDiff = new int[wordLength - 1];
		int i = 1;
	}
}