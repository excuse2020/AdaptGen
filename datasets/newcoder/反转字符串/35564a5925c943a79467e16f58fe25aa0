class Solution {
	String solve(String str) {
		if (str.length() > 0 && str.length() < 1000) {
			char[] charStr = str.toCharArray();
			String resultStr = "";
			for (int i = charStr.length - 1; i >= 0; i--) {
				resultStr += charStr[i];
			}
			return resultStr;
		} 
		else {
			return "字符串不合法";
		}
	}
}