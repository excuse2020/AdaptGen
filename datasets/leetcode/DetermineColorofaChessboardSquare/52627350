class Solution {
	boolean squareIsWhite(String coordinates) {
		int[][] chess = new int[8][8];
		for (int i = 0; i < 8; i++) {
			if (i % 2 == 0) {
				for (int j = 0; j < 8; j++) {
					if (j % 2 == 0) {
						chess[i][j] = 1;
					}
				}
			} 
			else {
				for (int j = 0; j < 8; j++) {
					if (j % 2 == 1) {
						chess[i][j] = 1;
					}
				}
			}
		}
		int x;
	}
}