class Solution {
	int maxNumberOfBalloons(String text) {
		String s = text;
		String a = "balloon";
		ArrayList<Character> l = new ArrayList<>();
		for (int i = 0; i < s.length(); i++) {
			if (a.contains(Character.toString(s.charAt(i)))) {
				l.add(s.charAt(i));
			}
		}
		int b = 0, a1 = 0, l1 = 0, o = 0, n = 0;
	}
}