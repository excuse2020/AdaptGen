class Solution {
	boolean poss(int[] freq, int[] freq2, int k) {
		if (freq[0] - freq2[0] >= k && freq[1] - freq2[1] >= k && freq[2] - freq2[2] >= k) {
			return true;
		}
		return false;
	}
	int takeCharacters(String s, int k) {
		int[] freq = new int[3];
		int n = s.length();
		int i = 0;
		int index;
	}
}