class Solution {
	int findParent(int child, int[] parent) {
		if (parent[child] == child) {
			return child;
		} 
		else {
			return parent[child] = findParent(parent[child],parent);
		}
	}
	String smallestEquivalentString(String s1, String s2, String baseStr) {
		int[] ch = new int[26];
		for (int i = 0; i < 26; i++) {
			ch[i] = i;
		}
		int len = s1.length();
		int ind = 0;
		int ch1;
	}
}