class Solution {
	int findMin(int a, int b, int l, int o, int n) {
		int min = a;
		if (b < min) {
			min = b;
		}
		if (l / 2 < min) {
			min = l / 2;
		}
		if (o / 2 < min) {
			min = o / 2;
		}
		if (n < min) {
			min = n;
		}
		return min;
	}
	int maxNumberOfBalloons(String text) {
		int aCount = 0, bCount = 0, oCount = 0, lCount = 0, nCount = 0;
		int i = 0;
	}
}