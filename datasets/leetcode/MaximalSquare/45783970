class Solution {
	int maximalSquare(char[][] m) {
		int r = m.length;
		int c = m[0].length;
		int max = 0;
		int[][] mat = new int[m.length][m[0].length];
		int[][] dp = new int[r + 1][c + 1];
		for (int i = 0; i < r; i++) {
			dp[i][0] = 0;
		}
		for (int i = 0; i < c; i++) {
			dp[0][i] = 0;
		}
		int i = 0;
		int j = 0;
	}
}