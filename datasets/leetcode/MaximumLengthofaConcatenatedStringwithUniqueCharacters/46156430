class Solution {
	int helper(List<String> arr, int[] dp, int index) {
		if (index >= arr.size()) {
			int count = 0;
			for (int i: dp) {	
				count += i;
			}
			return count;
		}
		int taken = 0;
		boolean should_take = true;
		int i = 0;
	}
	int maxLength(List<String> arr) {
		return helper(arr,new int[26],0);
	}
}