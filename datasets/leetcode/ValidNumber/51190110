class Solution {
	boolean isNumber(String s) {
		int exponentPlace;
	}
	boolean validateDecimal(String s, int start, int end) {
		if (start > end) {
			return false;
		}
	}
	boolean validateDigits(String s, int start, int end) {
		if (start > end) {
			return false;
		}
		for (int i = start; i <= end; i++) {
			if (!Character.isDigit(s.charAt(i))) {
				return false;
			}
		}
		return true;
	}
	boolean validateInteger(String s, int start, int end) {
		if (start > end) {
			return false;
		}
	}
}