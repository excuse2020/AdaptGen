class Solution {
	int lengthOfLongestSubstring(String s) {
		int i = 0;
		tint j = 0;
		tint max = 0;
		tHashSet<Character> set = new HashSet<Character>();
		if (!set.contains(s.charAt(j))) {
			set.add(s.charAt(j));
		}
		j++;
		t max = Math.max(set.size(),max);
		set.remove(s.charAt(i));
		i++;
		return max;
	}
}