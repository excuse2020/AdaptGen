class Solution {
	boolean squareIsWhite(String coordinates) {
		if (coordinates.charAt(0) % 2 != 0 && Integer.valueOf(coordinates.charAt(1)) % 2 != 0) {
			return false;
		}
		if (coordinates.charAt(0) % 2 == 0 && Integer.valueOf(coordinates.charAt(1)) % 2 == 0) {
			return false;
		}
		return true;
	}
}