class Solution {
	int count(String s, char ch) {
		int c = 0;
		int i = 1;
		while (i < s.length() - 1) {
			if (s.charAt(i) == ch && s.charAt(i - 1) == ch && s.charAt(i + 1) == ch) {
				c++;
			}
			i++;
		}
		return c;
	}
	boolean winnerOfGame(String colors) {
		boolean res = false;
		int aC;
	}
}