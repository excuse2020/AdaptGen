class Solution {
	boolean isNumber(String s) {
		char[] arr = s.toLowerCase().toCharArray();
		return isValidNumber(arr,0,arr.length - 1,true,false,true,true,false);
	}
	boolean isValidNumber(char[] arr, int i, int j, boolean canContainSign, boolean canBeEmpty, boolean canContainDot, boolean canContainE, boolean seenNumber) {
		if (i > j) {
			return canBeEmpty;
		}
		int ii = i;
	}
}