class Solution {
	long mod;
	int numberOfArrays(String s, int k) {
		mod = (int) 1e9 + 7;
		long[] dp = new long[s.length() + 1];
		Arrays.fill(dp,-1);
		return (int) solve(0,s,k,dp);
	}
	long solve(int idx, String s, int k, long[] dp) {
		if (idx == s.length()) {
			return 1;
		}
		if (dp[idx] != -1) {
			return dp[idx];
		}
		long max = 0, number = 0;
		int i = idx;
		int temp;
	}
}