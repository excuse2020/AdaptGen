class Solution {
	long mod = (long) (1e9 + 7);
	int countVowelPermutation(int n) {
		HashMap<Character, Integer> map = new HashMap<>();
	}
	long rec(int n, char prev, Long dp, HashMap<Character, Integer> map) {
		if (n == 0) {
			return 1;
		}
		if (dp[n][map.get(prev)] != null) {
			return dp[n][map.get(prev)];
		}
		long takeA;
	}
}