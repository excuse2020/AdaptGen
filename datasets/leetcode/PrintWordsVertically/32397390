class Solution {
	List<String> printVertically(String s) {
		String[] a = s.split(" ");
		int max = -1;
		for (String str: a) {	
			if (str.length() > max) {
				max = str.length();
			}
		}
		ArrayList<String> lst = new ArrayList<>();
		String add = "";
		int i = 0;
		for (String str: a) {	
			if (i >= str.length()) {
				add = add + " ";
			} 
			else {
				add = add + str.charAt(i);
			}
		}
	}
}