class Solution {
	double findMedianSortedArrays(int[] nums1, int[] nums2) {
		tint arr = new int[nums1.length + nums2.length];
		tdouble ans = 0;
		int index = 0;
		int i = 0;
		t arr = nums1[i];
		index++;
		int i = 0;
		t arr = nums2[i];
		index++;
		Arrays.sort(arr);
		tint len = arr.length;
		tint a = len / 2;
		if (len % 2 != 0) {
			int val = (len + 1) / 2;
			t ans = arr[val - 1];
			double val = (arr[a - 1] + arr[a]);
			t val = val / 2;
			ans = val;
		}
		return ans;
	}
}