class Solution {
	ArrayList<Integer> GetLeastNumbers_Solution(int[] input, int k) {
		ArrayList<Integer> arrayalist = new ArrayList<Integer>();
		Arrays.sort(input);
		if (k > input.length || k <= 0) {
			return arrayalist;
		} 
		else {
			for (int i = 0; i < k; i++) {
				arrayalist.add(input[i]);
			}
			return arrayalist;
		}
	}
}