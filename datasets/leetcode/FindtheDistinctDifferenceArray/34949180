class Solution {
	int[] distinctDifferenceArray(int[] nums) {
		int n = nums.length;
		int[] d = new int[n];
		for (int i = 0; i < n; i++) {
			Set<Integer> pre = new HashSet<>();
			for (int j = 0; j <= i; j++) {
				pre.add(nums[j]);
			}
			Set<Integer> suf = new HashSet<>();
			for (int j = i + 1; j < n; j++) {
				suf.add(nums[j]);
			}
			d[i] = pre.size() - suf.size();
		}
		return d;
	}
}