class Solution {
	String truncateSentence(String s, int k) {
		String[] stringArray = s.split("\\\\s+");
		StringBuilder finalString = new StringBuilder();
		for (int i = 0; i < k; i++) {
			finalString.append(stringArray[i]);
			finalString.append(" ");
		}
		return finalString.toString().trim();
	}
}