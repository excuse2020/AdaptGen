class Solution {
	Pattern NUMBER_PATTERN = Pattern.compile("^[+-]?(\\\\d*\\\\.?\\\\d+|\\\\d+\\\\.\\\\d*)([eE][+-]?\\\\d+)?$");
	boolean isNumber(String s) {
		int length = s.length();
		boolean hasDecimal = false;
		boolean hasDigit = false;
		boolean hasExponent = false;
		int i = 0;
		char c = s.charAt(i);
		hasDigit = true;
	}
}