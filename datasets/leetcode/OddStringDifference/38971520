class Solution {
	int[] calc(String str, Map<Character, Integer> map) {
		int n = str.length();
		int[] arr = new int[n - 1];
		for (int i = 0; i < n - 1; i++) {
			arr[i] = map.get(str.charAt(i + 1)) - map.get(str.charAt(i));
		}
		return arr;
	}
	String oddString(String[] words) {
		Map<Character, Integer> map = new HashMap<>();
		char a;
	}
}