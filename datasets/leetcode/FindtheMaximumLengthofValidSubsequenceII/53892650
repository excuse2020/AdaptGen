class Solution {
	int maximumLength(int[] nums, int k) {
		int[][] dp = new int[nums.length + 1][k + 1];
		int longest = 0;
		for (int i = 0; i < nums.length; i++) {
			for (int j = 0; j < i; j++) {
				int val = (nums[i] + nums[j]) % k;
				dp[i][val] = dp[j][val] + 1;
				longest = Math.max(longest,dp[i][val]);
			}
		}
		return longest + 1;
	}
}