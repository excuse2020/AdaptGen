class Solution {
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		int word1 = numericalValue(firstWord);
		int word2 = numericalValue(secondWord);
		int word3 = numericalValue(targetWord);
		if ((word1 + word2) == word3) {
			return true;
		} 
		else {
			return false;
		}
	}
	int numericalValue(String str) {
		int res = 0;
		char ch1;
	}
}