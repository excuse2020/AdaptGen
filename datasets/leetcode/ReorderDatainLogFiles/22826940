class Solution {
	String[] processLetterLog(String[] logArr, int logIdx) {
		String[] res = new String[3];
		res[0] = Integer.toString(logIdx);
		res[1] = logArr[0];
		StringBuilder sb = new StringBuilder();
		for (int i = 1; i < logArr.length; i++) {
			sb.append(logArr[i] + ",");
		}
		res[2] = sb.toString();
		return res;
	}
	String[] reorderLogFiles(String[] logs) {
		String[] res = new String[logs.length];
		PriorityQueue<String[]> pq;
	}
}