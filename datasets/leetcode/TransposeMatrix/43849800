class Solution {
	int[][] transpose(int[][] matrix) {
		int row = matrix.length;
		int col = matrix[0].length;
		int transposed_matrix = new int[col][row];
		for (int i = 0; i < col; i++) {
			for (int j = 0; j < row; j++) {
				transposed_matrix[i][j] = matrix[j][i];
			}
		}
		return transposed_matrix;
	}
}