class Solution {
	int sumOfMultiples(int n) {
		int sum = 0;
		int x = n / 3;
		sum += 3 * x / 2;
		x = n / 5;
		sum += 5 * x / 2;
		x = n / 7;
		sum += 7 * x / 2;
		x = n / 15;
		sum -= 15 * x / 2;
		x = n / 21;
		sum -= 21 * x / 2;
		x = n / 35;
		sum -= 35 * x / 2;
		x = n / 105;
		sum += 105 * x / 2;
		return sum;
	}
}