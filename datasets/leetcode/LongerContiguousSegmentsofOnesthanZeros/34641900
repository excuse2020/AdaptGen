class Solution {
	boolean checkZeroOnes(String s) {
		if (s == null || s.length() == 0) {
			return false;
		}
		int i = 0;
		int j = 0;
		char c;
		int count = 0;
		int longest_count_0 = 0;
		int longest_count_1 = 0;
		c = s.charAt(i);
		count += 1;
		j = i + 1;
		while (j < s.length() && s.charAt(j) == c) {
			count += 1;
			j++;
		}
	}
}