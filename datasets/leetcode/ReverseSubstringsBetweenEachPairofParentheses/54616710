class Solution {
	String reverse(int start, int end, String value) {
		StringBuilder a = new StringBuilder(value.substring(start,end + 1));
		return a.reverse().toString();
	}
	String reverseParentheses(String s) {
		boolean noBrackets = false;
		int openBrack = -1;
		if (!s.contains("(")) {
			noBrackets = true;
		}
		int i = 0;
	}
}