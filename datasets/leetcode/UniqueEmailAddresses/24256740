class Solution {
	int numUniqueEmails(String[] emails) {
		Set<String> transformedUniqueEmails = new HashSet<>();
		for (String email: emails) {	
			String transformedEmail = transform(email);
			transformedUniqueEmails.add(transformedEmail);
		}
		return transformedUniqueEmails.size();
	}
	String transform(String email) {
		StringBuilder transformedEmail = new StringBuilder();
		int idx;
	}
}