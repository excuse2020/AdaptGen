class Solution {
	void btrack(List<String> res, String sub, String digits, int idx) {
		if (idx == digits.length()) {
			res.add(sub);
			return;
		}
		int run;
		int len = 3;
	}
	List<String> letterCombinations(String digits) {
		List<String> list = new ArrayList<>();
		if (digits.length() == 0) {
			return list;
		}
		btrack(list,"",digits,0);
		return list;
	}
}