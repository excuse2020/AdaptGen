class Solution {
	int helper(String s, int i, int prev, int dp) {
		if (i >= s.length()) {
			return 0;
		}
		if (prev != -1 && dp[i][prev] != -1) {
			return dp[i][prev];
		}
		int to_one = Integer.MAX_VALUE;
		int to_zero = Integer.MAX_VALUE;
	}
	int minFlipsMonoIncr(String s) {
		int one = 0;
		int zero = 0;
	}
}