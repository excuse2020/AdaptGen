class Solution {
	String restoreString(String s, int[] indices) {
		@r int n = indices.length;
		@r char ch = s.toCharArray();
		@r String str = "";
		for (int i = 0; i < n - 1; i++) {
			for (int j = i + 1; j < n; j++) {
				if (indices[i] > indices[j]) {
				}
			}
		}
		r indices = indices[i] + indices[j] - (indices[j] = indices[i]);
		@r char temp = ch[i];
		ch[i] = ch[j];
		ch[j] = temp;
		for (char i: ch) {	
		}
		r str = str + i;
		return str;
	}
}