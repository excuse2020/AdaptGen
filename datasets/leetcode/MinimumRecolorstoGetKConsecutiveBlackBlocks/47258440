class Solution {
	int minWBlocks(String blocks, int k, int i, int e, int mn, int n) {
		if (e > n) {
			System.out.println(mn);
			return mn;
		}
		int minW = 0;
		int j = i;
	}
	int minimumRecolors(String blocks, int k) {
		int n = blocks.length();
		return minWBlocks(blocks,k,0,k,Integer.MAX_VALUE,n);
	}
}