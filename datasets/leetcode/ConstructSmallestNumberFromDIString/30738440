class Solution {
	void reverse(int arr, int l, int h) {
		while (l < h) {
			int temp = arr[l];
			arr[l] = arr[h];
			arr[h] = temp;
			l++;
			h--;
		}
	}
	String smallestNumber(String str) {
		int i = 0, j = 0, n = str.length(), cnt = 1;
		int[] arr = new int[n + 1];
		for (int p = 0; p <= n; p++) {
			arr[p] = p + 1;
		}
	}
}