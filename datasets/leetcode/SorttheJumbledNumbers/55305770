	}
	int[] sortJumbled(int[] mapping, int[] nums) {
		int[] ans = new int[nums.length];
		List<int[]> A = new ArrayList<>();
		for (int i = 0; i < nums.length; ++i) {
			A.add(new int{getMapped(nums[i],mapping), i, nums[i]});
		}
	}
