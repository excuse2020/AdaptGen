class Solution {
	int search(int[] nums, int target) {
		int index = -1;
		int min = 0;
		int max = nums.length - 1;
		int mid = 0;
		while (min <= max) {
			mid = min + (max - min) / 2;
			if (nums[mid] > target) {
				max = mid - 1;
			} 
			else if (nums[mid] < target) {
				min = mid + 1;
			} 
			else {
				index = mid;
				max = mid - 1;
			}
		}
		return index;
	}
}