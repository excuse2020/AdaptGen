class Solution {
	int min(int[] prefix1, int[] prefix2) {
		int res = Integer.MAX_VALUE;
		for (int i = 2; i < prefix2.length; i++) {
			res = Math.min(res,prefix1[26] - prefix1[i - 1] + prefix2[i - 1]);
		}
		return res;
	}
	int minCharacters(String a, String b) {
		int[] c1 = new int[26];
		int[] c2 = new int[26];
		int i = 0;
	}
}