class Solution {
	List<String> findAndReplacePattern(String[] words, String pattern) {
		List<String> ans = new ArrayList<>();
		for (String word: words) {	
			if (isSamePattern(word,pattern)) {
				ans.add(word);
			}
		}
		return ans;
	}
	boolean isSamePattern(String word, String pattern) {
		char[] ch = new char[26];
		int i = 0;
	}
}