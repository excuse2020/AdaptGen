class Solution {
	String truncateSentence(String s, int k) {
		String st = s.split(" ");
		List<String> list = new ArrayList<>();
		for (int i = 0; i < st.length; i++) {
			if (i < k) {
				list.add(st[i]);
			}
		}
		System.out.println(list);
		String finalValue = String.join(" ",list);
		return finalValue;
	}
}