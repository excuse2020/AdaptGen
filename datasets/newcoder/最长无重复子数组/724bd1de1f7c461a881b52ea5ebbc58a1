class Solution {
	int maxLength(int[] arr) {
		HashSet<integer> se = new HashSet() & lt;
		byte gt;
		int l = 0, r = 0;
		int cnt = 1;
		while (l & lt(arr.length & amp,r & lt,arr.length)) {
		}
		if (se.contains(arr[r]) == false) {
			se.add(arr[r++]);
			cnt = Math.max(r - l,cnt);
		} 
		else {
			se.remove(arr[l++]);
		}
		return cnt;
	}
}