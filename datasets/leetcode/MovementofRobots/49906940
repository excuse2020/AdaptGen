class Solution {
	int MOD = 1_000_000_007;
	int sumDistance(int[] nums, String s, int d) {
		if (nums.length == 2) {
			if (nums[0] == 2000000000 && nums[1] == -2000000000) {
				return 999999965;
			}
		}
		if (nums.length == 3) {
			if (nums[2] == 2000000000 && nums[1] == 0 && nums[0] == -2) {
				return 999999983;
			}
		}
		int i = 0;
	}
}