class Solution {
	String solve(String str) {
		if (str == null) {
			return null;
		}
		char[] chars = str.toCharArray();
		StringBuilder stringBuilder = new StringBuilder(chars.length);
		for (int i = chars.length - 1; i >= 0; i--) {
			stringBuilder.append(chars[i]);
		}
		return stringBuilder.toString();
	}
}