class Solution {
	int[] productExceptSelf(int[] nums) {
		int pre = 1;
		int post = 1;
		int res = new int[nums.length];
		for (int i = 0; i < nums.length; i++) {
			res[i] = 1;
		}
		for (int i = 0; i < nums.length; i++) {
			res[i] *= pre;
			pre *= nums[i];
		}
		for (int i = nums.length - 1; i >= 0; i--) {
			res[i] *= post;
			post *= nums[i];
		}
		return res;
	}
}