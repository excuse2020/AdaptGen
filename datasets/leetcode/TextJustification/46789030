class Solution {
	IList<string> FullJustify(string[] words, int maxWidth) {
		var sentence = new List<string>();
		var line = new List<string>();
		var length = 0;
		var index = 0;
		var word = words[index];
		var extraSpaces = maxWidth - length;
		int spaces = extraSpaces / Math.Max((line.Count() - 1),1);
		int leftOver = extraSpaces % Math.Max((line.Count() - 1),1);
		int j = 0;
	}
}