class Solution {
	boolean isSumEqual(String firstWord, String secondWord, String targetWord) {
		int fs = firstWord.length(), ss = secondWord.length(), ts = targetWord.length();
		int max = 0;
		if (fs > ts && fs > ss) {
			max = fs;
		} 
		else if (ss > fs && ss > ts) {
			max = ss;
		} 
		else {
			max = ts;
		}
		int l = 0, m = 0, n = 0;
		int i = 0;
	}
}