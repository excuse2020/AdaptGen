class Solution {
	List<String> printVertically(String s) {
		String[] words = s.split(" ");
		List<String> ans = new ArrayList<>();
		int maxLen = 0;
		StringBuilder sb = new StringBuilder();
		for (String word: words) {	
			if (word.length() > maxLen) {
				maxLen = word.length();
			}
		}
		int j = 0;
		int i = 0;
		sb.append(words[i].charAt(j));
	}
}