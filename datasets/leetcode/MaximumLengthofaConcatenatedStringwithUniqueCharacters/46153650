	}
	int help(int idx, List<String> arr, int[] selected, int len) {
		if (idx == arr.size()) {
			return len;
		}
		String cStr = arr.get(idx);
		return help(idx + 1,arr,selected,len);
		int j = 0;
	}
	int maxLength(List<String> arr) {
		int[] selected = new int[26];
		return help(0,arr,selected,0);
	}
}