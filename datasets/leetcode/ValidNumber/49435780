class Solution {
	boolean isNumber(String s) {
		s = s.toLowerCase();
	}
	boolean isValidDecimal(String s) {
		boolean dotSeen = false;
		boolean numSeen = false;
		char[] c = s.toCharArray();
		int index = 0;
	}
	boolean isValidInteger(String s) {
		boolean numSeen = false;
		char[] c = s.toCharArray();
		int index = 0;
	}
}