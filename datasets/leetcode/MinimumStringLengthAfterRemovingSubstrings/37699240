class Solution {
	int minLength(String s) {
		if (s.contains("AB") == false && s.contains("CD") == false) {
			return s.length();
		}
		char[] ch = s.toCharArray();
		ArrayList<Character> ch1 = new ArrayList<Character>();
		for (int i = 0; i < ch.length; i++) {
			ch1.add(ch[i]);
		}
		int j = 1;
	}
}