class Solution {
	boolean strongPasswordCheckerII(String password) {
		if (password.length() < 8) {
			return false;
		}
		boolean lwrCse = false, uprCse = false, dgt = false, splChr = false;
		int i = 0;
		char ch = password.charAt(i);
		if (i > 0 && ch == password.charAt(i - 1)) {
			return false;
		}
	}
}