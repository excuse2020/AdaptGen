class Solution {
	StringBuilder backTrack(int[] count, StringBuilder sb) {
		int size = sb.length();
		int start = 0;
		if (size == 0) {
			start = 2;
		}
	}
	String largestTimeFromDigits(int[] arr) {
		int[] count = new int[10];
		for (int num: arr) {	
			count[num]++;
		}
		StringBuilder sb = backTrack(count,new StringBuilder());
	}
}