class Solution {
	boolean canReach(String s, int minJump, int maxJump) {
		Queue<Integer> q = new LinkedList<Integer>();
		q.add(Character.getNumericValue(s.charAt(0)));
		int farthest = 0;
		int i, startIndex, stopIndex;
		i = q.poll();
		startIndex = Math.max(i + minJump,farthest + 1);
		stopIndex = Math.min(i + maxJump,s.length());
		int j = startIndex;
	}
}