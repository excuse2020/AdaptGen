class Solution {
	boolean isSimilar(boolean[] map1, boolean[] map2) {
		for (int i = 0; i < 26; i++) {
			if (map1[i] != map2[i]) {
				return false;
			}
		}
		return true;
	}
	int similarPairs(String[] words) {
		int count = 0, n = words.length;
		int i = 0;
		boolean[] map1 = new boolean[26];
		String s1 = words[i];
		int x = s1.length() - 1;
	}
}