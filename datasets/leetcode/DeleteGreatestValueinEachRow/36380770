class Solution {
	int deleteGreatestValue(int[][] grid) {
		int ans = 0;
		for (int a: grid) {	
			Arrays.sort(a);
		}
		for (int i = 0; i < grid[0].length; i++) {
			int max = Integer.MIN_VALUE;
			for (int j = 0; j < grid.length; j++) {
				if (max < grid[j][i]) {
					max = grid[j][i];
				}
			}
			ans += max;
		}
		return ans;
	}
}