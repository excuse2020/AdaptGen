class Solution {
	int numberOfSubmatrices(char[][] arr) {
		int n = arr.length;
		int m = arr[0].length;
		int arrX = new int[n][m];
		int arrY = new int[n][m];
		int i = 0;
		int j = 0;
		int a = i - 1 >= 0 ? arrX[i - 1][j] : 0;
		int b = j - 1 >= 0 ? arrX[i][j - 1] : 0;
		int c = (i - 1 >= 0 && j - 1 >= 0) ? arrX[i - 1][j - 1] : 0;
	}
}