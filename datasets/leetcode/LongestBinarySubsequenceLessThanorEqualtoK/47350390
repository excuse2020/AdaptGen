class Solution {
	int longestSubsequence(String s, int k) {
		int len = s.length();
		int sum = 0;
		int count = 0;
		int i = len - 1;
	}
	int solveMemo(String s, int idx, int[][] dp, int sum, int size, int k) {
		if (idx < 0) {
			return 0;
		}
		if (dp[idx][size] != -1) {
			return dp[idx][size];
		}
		int take = 0;
	}
	int solveRec(String s, int idx, int sum, int size, int k) {
		if (idx < 0) {
			return 0;
		}
		int take = 0;
	}
}