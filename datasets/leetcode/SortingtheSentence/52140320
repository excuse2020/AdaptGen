class Solution {
	void append(String s, StringBuilder result, int start, int end) {
		for (int i = start; i <= end; i++) {
			result.append(s.charAt(i));
		}
		result.append(" ");
	}
	String sortSentence(String s) {
		StringBuilder result = new StringBuilder();
		int indexStart = 0;
		int indexEnd = 0;
		int i = 1;
		int j = 0;
		indexStart = j;
	}
}