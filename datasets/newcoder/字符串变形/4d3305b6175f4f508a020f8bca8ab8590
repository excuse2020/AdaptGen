class Solution {
	String trans(String s, int n) {
		s = s + " k";
		String[] slist = s.split(" ");
		List<String> sl = Arrays.stream(slist).collect(Collectors.toCollection());
		Collections.reverse(sl);
		slist = sl.toArray(new String[sl.size()]);
		StringBuilder ans = new StringBuilder();
		int i = 0;
		String t;
	}
}