class Solution {
	int countValidWords(String sentence) {
		int noOfValidToken = 0;
		String[] sentence_part = sentence.trim().split("\\\\s+");
		for (String part: sentence_part) {	
			if (part.contains("0") || part.contains("1")) {
				continue;
			}
		}
		int positionOfHyphen = -1;
		int positionOfPunctuation = -1;
		int i = 0;
		char c = part.charAt(i);
	}
}