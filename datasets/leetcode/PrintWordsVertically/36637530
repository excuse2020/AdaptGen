class Solution {
	List<String> printVertically(String s) {
		ArrayList<String> list = new ArrayList<String>();
		String[] words = s.split(" ");
		int listSize = 0;
		for (String word: words) {	
			int len = word.length();
			if (len > listSize) {
				listSize = len;
			}
		}
		int i = 0;
		StringBuilder st = new StringBuilder();
		boolean empty = true;
		for (String word: words) {	
			empty = false;
			st.append(word.charAt(i));
		}
	}
}