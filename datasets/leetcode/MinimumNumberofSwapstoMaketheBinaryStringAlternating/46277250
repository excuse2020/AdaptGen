class Solution {
	int func(char[] c, char x) {
		int n = c.length;
		int count = 0;
		for (int i = 0; i < n; i++) {
			if (i % 2 == 0) {
				if (c[i] != x) {
					count++;
				}
			} 
			else {
				if (c[i] == x) {
					count++;
				}
			}
		}
		return count / 2;
	}
	int minSwaps(String s) {
		char[] c = s.toCharArray();
		int n = c.length;
		int o = 0;
		int z = 0;
		int i = 0;
	}
}