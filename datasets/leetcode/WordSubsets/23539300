class Solution {
	void prep(String s, HashMap<Integer, Integer> m) {
		char[] c = s.toCharArray();
		int[] fr = new int[26];
	}
	List<String> wordSubsets(String[] words1, String[] words2) {
		HashMap<Integer, Integer> m = new HashMap();
		for (String s: words2) {	
			prep(s,m);
		}
		List<String> ans = new ArrayList();
		boolean canAdd = true;
		char[] c = s.toCharArray();
		int[] fr = new int[26];
	}
}