class Solution {
	int takeCharacters(String s, int k) {
		if (k == 0) {
			return 0;
		}
		char[] arr = s.toCharArray();
		int l = s.length();
		ArrayList<Integer> list_a = new ArrayList<>();
		ArrayList<Integer> list_b = new ArrayList<>();
		ArrayList<Integer> list_c = new ArrayList<>();
		int i = 0;
		char c = arr[i];
	}
}