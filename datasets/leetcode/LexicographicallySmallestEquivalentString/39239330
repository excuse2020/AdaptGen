class Solution {
	int parent;
	int find(int i) {
		if (parent[i] == i) {
			return i;
		}
		return parent[i] = find(parent[i]);
	}
	String smallestEquivalentString(String s1, String s2, String baseStr) {
		parent = new int[26];
		for (int i = 0; i < 26; i++) {
			parent[i] = i;
		}
		int i = 0;
		int u;
	}
}