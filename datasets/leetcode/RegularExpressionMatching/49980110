class Solution {
	boolean isMatch(String s, String p) {
		char pChars = new char[p.length() + 1];
		char sChars = new char[s.length() + 1];
	}
	boolean recursion(int i, int j, char p, char s, int[][] dp) {
		if (i > p.length - 1 && j > s.length - 1) {
			return true;
		}
		if (i > p.length - 1 || j > s.length - 1) {
			return false;
		}
		boolean result = false;
		if (dp[i][j] == -1) {
			return false;
		}
		if (dp[i][j] == 1) {
			return true;
		}
	}
}