class Solution {
	int[] productQueries(int n, int[][] queries) {
		int len = queries.length;
		int mod = (int) 1e9 + 7;
		int[] ans = new int[len];
		String s = Integer.toBinaryString(n);
		ArrayList<Integer> list = new ArrayList<>();
		int x = s.length() - 1;
		int i = s.length() - 1;
		char c = s.charAt(i);
	}
}