class Solution {
	void reverse(int[] nums, int start, int end) {
		while (start < end) {
			int temp = nums[start];
			nums[start] = nums[end];
			nums[end] = temp;
			start++;
			end--;
		}
	}
	String smallestNumber(String pattern) {
		int n = pattern.length();
		int[] nums = new int[n + 1];
		for (int i = 0; i <= n; i++) {
			nums[i] = i + 1;
		}
		int i = 0;
	}
}