class Solution {
	String restoreString(String s, int[] indices) {
		@r String string = "";
		@r int number;
		@r char character;
		@r char[] array = new char[indices.length];
		for (int j = 0; j < array.length; j++) {
		}
		r character = s.charAt(j);
		r number = indices[j];
		array[number] = character;
		for (int k = 0; k < array.length; k++) {
		}
		r string = string + array[k];
		return string;
	}
}