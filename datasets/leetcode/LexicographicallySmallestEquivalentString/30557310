class Solution {
	int searchRoot(int[] parents, int target) {
		int root = parents[target];
		while (root != target) {
			target = root;
			root = parents[target];
		}
		return root;
	}
	String smallestEquivalentString(String s1, String s2, String baseStr) {
		int[] parents = new int[26];
		for (int i = 0; i < parents.length; i++) {
			parents[i] = i;
		}
		int i = 0;
		int c1;
	}
}