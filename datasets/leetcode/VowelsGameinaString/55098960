class Solution {
	boolean containVowels(String s) {
		String str = s.toLowerCase();
		String vowels = "aeiou";
		for (int i = 0; i < str.length(); i++) {
			char ch = str.charAt(i);
			if (vowels.indexOf(ch) != -1) {
				return true;
			}
		}
		return false;
	}
	boolean doesAliceWin(String s) {
		return containVowels(s);
	}
}