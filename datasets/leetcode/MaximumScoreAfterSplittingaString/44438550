class Solution {
	int calZeroandOne(char[] array, int start, int end, char find) {
		int count = 0;
		for (int i = start; i <= end; i++) {
			if (array[i] == find) {
				count++;
			}
		}
		return count;
	}
	int maxScore(String s) {
		char[] binaryArr = s.toCharArray();
		int maxScore = 0;
		int i = 0;
		int score;
	}
}