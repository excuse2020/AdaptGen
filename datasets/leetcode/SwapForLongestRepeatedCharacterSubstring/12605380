class Solution {
	int maxRepOpt1(String text) {
		int[] count = new int[26];
		List<Pair<Character, Integer>> list = new ArrayList<>();
		int i = 0;
		char c = text.charAt(i);
		int freq = 0;
		while (i < text.length() && text.charAt(i) == c) {
			i++;
			freq++;
		}
		list.add(new Pair(c, freq));
	}
}