class Solution {
	int[] applyOperations(int[] nums) {
		for (int i = 0; i + 1 < nums.length; i++) {
			if (nums[i] == nums[i + 1]) {
				nums[i] = nums[i] * 2;
				nums[i + 1] = 0;
			}
		}
		int index = 0;
		for (int i = 0; i < nums.length; i++) {
			if (nums[i] != 0) {
				nums[index++] = nums[i];
			}
		}
		while (index < nums.length) {
			nums[index++] = 0;
		}
		return nums;
	}
}