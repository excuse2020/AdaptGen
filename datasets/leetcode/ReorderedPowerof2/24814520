class Solution {
	boolean checkPow(int n) {
		if (n % 2 != 0) {
			return false;
		}
		while (n > 1) {
			if (n % 2 != 0) {
				return false;
			}
			n = n / 2;
		}
		return true;
	}
	boolean getAllNums(String input, boolean[] visited, String curr) {
	}
	boolean reorderedPowerOf2(int n) {
		if (n == 1) {
			return true;
		}
		String input = n + "";
		boolean[] visited = new boolean[input.length()];
		return getAllNums(input,visited,"");
	}
}