class Solution {
	int countValidWords(String sentence) {
		String[] words = sentence.split(" ");
		int count = 0;
		for (int i = 0; i < words.length; i++) {
			if (isValid(words[i])) {
				count++;
			}
		}
		return count;
	}
	boolean isDigit(char current) {
	}
	boolean isNotLetter(char current) {
	}
	boolean isPunctuation(char current) {
	}
	boolean isValid(String str) {
		if (str.isEmpty()) {
			return false;
		}
		int countOfHyphen = 0;
		int i = 0;
		char current = str.charAt(i);
		if (isDigit(current)) {
			return false;
		}
	}
}