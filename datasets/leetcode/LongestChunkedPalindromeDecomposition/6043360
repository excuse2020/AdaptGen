class Solution {
	int MOD = 1_000_000_007;
	int decomposition(String text, int lo, int hi) {
		if (hi < lo) {
			return 0;
		}
		if (hi == lo) {
			return 1;
		}
		long base = 1;
		long hash1 = 0L, hash2 = 0L;
	}
	int longestDecomposition(String text) {
		return decomposition(text,0,text.length() - 1);
	}
}