class Solution {
	int mod = (int) (1e9) + 7;
	long countWays(char[] arr, int k, int i, int n, long[] dp) {
		if (i == n) {
			return 1;
		}
	}
	boolean isValid(char[] arr, int i, int j, int k) {
		long val = 0;
		int count = 0;
	}
	int numberOfArrays(String s, int k) {
		char[] arr = s.toCharArray();
		int n = arr.length;
		long[] dp = new long[n];
		Arrays.fill(dp,-1);
		return (int) (countWays(arr,k,0,n,dp) % mod);
	}
}