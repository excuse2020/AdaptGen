class Solution {
	int[] platesBetweenCandles(String s, int[][] queries) {
		int n;
		n = s.length();
		int[] platesBtwnCandles = new int[n];
		int[] ans = new int[queries.length];
		if (n <= 0) {
			return ans;
		}
		int i = 0;
		int j = -1;
		platesBtwnCandles[i] = i == 0 ? 0 : platesBtwnCandles[i - 1];
	}
}