class Solution {
	String shortestPalindrome(String s) {
		if (s.isEmpty()) {
			return s;
		}
		int index = zAlgo(s);
		StringBuilder reverseString = new StringBuilder(s).reverse();
		StringBuilder result = new StringBuilder(reverseString.substring(0,s.length() - index));
		result.append(s);
		return result.toString();
	}
	int zAlgo(String s) {
		StringBuilder str = new StringBuilder(s);
	}
}