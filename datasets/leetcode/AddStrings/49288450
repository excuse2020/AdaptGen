class Solution {
	String addStrings(String num1, String num2) {
		if (num1 == null || num1.length() == 0) {
			return num2;
		}
		if (num2 == null || num2.length() == 0) {
			return num1;
		}
		int carry = 0;
		int i = num1.length() - 1, j = num2.length() - 1;
		StringBuilder sumStr = new StringBuilder();
		int firstNum;
	}
}