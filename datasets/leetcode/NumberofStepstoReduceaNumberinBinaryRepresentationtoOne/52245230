class Solution {
	StringBuilder add(StringBuilder s) {
		BigInteger b1 = new BigInteger(s.toString(), 2);
		BigInteger b2 = new BigInteger("1", 2);
		BigInteger ans = b1.add(b2);
		return new StringBuilder(ans.toString(2));
	}
	StringBuilder divide(StringBuilder s) {
		s = s.deleteCharAt(s.length() - 1);
		return s;
	}
	int numSteps(String s) {
		int ans = 0;
		StringBuilder x = new StringBuilder(s);
	}
}