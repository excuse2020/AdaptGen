class Solution {
	int numUniqueEmails(String[] emails) {
		boolean ignoreChar;
		boolean readingDomainName;
		Set<String> uniqueEmails = new HashSet<>();
		for (String email: emails) {	
			ignoreChar = false;
		}
		readingDomainName = false;
		StringBuilder realEmail = new StringBuilder();
		int i = 0;
		char character = email.charAt(i);
		realEmail.append(character);
	}
}