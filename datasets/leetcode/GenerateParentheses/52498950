class Solution {
	void generate(String curr, int n, int length, List<String> result) {
		if (length == 2 * n) {
			if (isValid(curr)) {
				result.add(curr);
			}
			return;
		}
	}
	List<String> generateParenthesis(int n) {
		List<String> result = new ArrayList<>();
		generate("",n,0,result);
		return result;
	}
