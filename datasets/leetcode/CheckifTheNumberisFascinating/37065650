class Solution {
	boolean isFascinating(int n) {
		int a = n * 2;
		int b = n * 3;
		int[] digits = new int[3];
		digits[0] = n;
		digits[1] = a;
		digits[2] = b;
		StringBuilder sb = new StringBuilder();
		for (int digit: digits) {	
			sb.append(digit);
		}
		Set<Character> set = new HashSet<>();
		int i = 0;
	}
}