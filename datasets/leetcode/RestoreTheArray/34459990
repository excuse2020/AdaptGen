class Solution {
	int MOD = (int) (1e9 + 7);
	Integer dp;
	int countWays(String str, int max, int idx) {
		if (idx == str.length()) {
			return 1;
		}
		if (dp[idx] != null) {
			return dp[idx];
		}
	}
	int numberOfArrays(String s, int k) {
		dp = new Integer[s.length() + 1];
		return countWays(s,k,0);
	}
}