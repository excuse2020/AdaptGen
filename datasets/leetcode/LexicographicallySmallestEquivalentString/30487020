class Solution {
	int find(int sets, int n) {
		int t = n;
		while (sets[t] != t) {
			t = sets[t];
		}
		sets[n] = t;
		return sets[n];
	}
	String smallestEquivalentString(String s1, String s2, String baseStr) {
		int sets = new int[26];
		for (int i = 0; i < 26; i++) {
			sets[i] = i;
		}
		int i = 0;
		int a;
	}
}