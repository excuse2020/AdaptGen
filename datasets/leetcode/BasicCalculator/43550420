class Solution {
	int calculate(String s) {
		if (s.length() == 0) {
			return 0;
		}
		ArrayList<Integer> num = new ArrayList<>();
		ArrayList<Character> oper = new ArrayList<>();
		int i = 0;
	}
	int stringToInteger(String str) {
		int ans = 0;
		for (int i = 0; i < str.length(); i++) {
			ans += (str.charAt(str.length() - i) - 48) * Math.pow(10,i);
		}
		return ans;
	}
}