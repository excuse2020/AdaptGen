class Solution {
	String replaceSpace(String s) {
		StringBuilder stringbuilder = new StringBuilder();
		for (int i = 0; i < s.length(); i++) {
			if (s.charAt(i) == ' ') {
				stringbuilder.append("%20");
			} 
			else {
				stringbuilder.append(s.charAt(i));
			}
		}
		return stringbuilder.toString();
	}
}