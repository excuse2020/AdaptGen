		int n = source.length();
		int[][] distmatrix = new int[26][26];
		for (int i = 0; i < 26; i++) {
			Arrays.fill(distmatrix[i],Integer.MAX_VALUE);
			distmatrix[i][i] = 0;
		}
		int i = 0;
		int from;
	}
}