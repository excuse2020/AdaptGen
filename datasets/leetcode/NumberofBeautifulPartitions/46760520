class Solution {
	int MOD = 1_000_000_007;
	int[][] dp;
	int beautifulPartitions(String s, int k, int minLength) {
		dp = new int[s.length()][k + 1];
		for (int[] row: dp) {	
			Arrays.fill(row,-1);
		}
	}
	int recursive(int k, int minlength, int index, String str) {
		if (index == str.length()) {
			return 0;
		}
	}
}