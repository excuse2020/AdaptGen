class Solution {
	int getNums(int n, int m) {
		int mod = 1000000007;
		long b = 1;
		b = (b * n) % mod;
		System.out.println(b);
		b = (b * m) % mod;
		System.out.println(b);
		b = (b * (n - 1)) % mod;
		System.out.println(b);
		b = (b * (m - 1)) % mod;
		System.out.println(b);
		System.out.println(b % mod);
		return (int) b;
	}
}