class Solution {
	void getMax(String s, int i, int max, int arr) {
		if (i == s.length() - 1) {
			System.out.println(max);
			arr[0] = max;
			return;
		}
		String left = s.substring(0,i + 1);
		String right = s.substring(i + 1);
		int countL = 0;
		int j = 0;
	}
	int maxScore(String s) {
		int max = -9999;
		int arr = new int[1];
		getMax(s,0,max,arr);
		return arr[0];
	}
}